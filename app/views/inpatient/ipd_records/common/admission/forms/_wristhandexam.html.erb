              <%= opdrecord.read_attribute(:"r_rom1stmcpextnflexn") %>
              ,
          <% end %>
          <% if opdrecord.romsliderrangeleft1stmcpextnflexn.length > 0 && opdrecord.romsliderrangeleft1stmcpextnflexn != "0 : 50" %>
              <b>Left  1st MCP:Extn-Flexn:</b>
              <%= opdrecord.read_attribute(:"romsliderrangeleft1stmcpextnflexn") %>
              <%= opdrecord.read_attribute(:"l_rom1stmcpextnflexn") %>

          <% end %>

          <% if opdrecord.romsliderrangeright1stipextnflexn.length > 0 && opdrecord.romsliderrangeright1stipextnflexn != "0 : 80" %>
              <b>Right 1st IP:Extn-Flexn:</b>
              <%= opdrecord.read_attribute(:"romsliderrangeright1stipextnflexn") %>
              <%= opdrecord.read_attribute(:"r_rom1stipextnflexn") %>
