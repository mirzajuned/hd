        <% examination_counter = examination_counter+1 %>
    <% end %>
    <% unless opdrecord.objectivecomments.blank? %>
        <% examination_counter = examination_counter+1 %>
    <% end %>

    <!--counter code ends here-->

    <div class="examination mb5" id="examination">
      <%if examination_counter > 0 %>

          <div class="row">
            <strong>EXAMINATION :</strong>
            <div class="summary-line-border"></div>

          </div>
      <%end%>

      <div class="row">

        <div class="col-mb-12">

          <% unless opdrecord.generalsigns.blank? %>
              <b>General Signs :</b>
              <%= opdrecord.read_attribute(:"generalsigns") %>
              |
          <% end %>
          <% unless opdrecord.areaaffectedobjective.blank? %>
              <b>Area Affected :</b>
              <%= opdrecord.read_attribute(:"areaaffectedobjective") %>
              |
          <% end %>
          <% unless opdrecord.sideobjective.blank? %>
              <b>Side :</b>
              <%= opdrecord.read_attribute(:"sideobjective") %>
              |
          <% end %>
          <% unless opdrecord.location.blank? %>
              <b>Location :</b>
              <%= opdrecord.read_attribute(:"location") %>
              |
          <% end %>
          <% unless opdrecord.skincondition.blank? %>
              <% unless opdrecord.skincondition == "Normal" %>
                  <b>Skin Condition :</b>
                  <%= opdrecord.read_attribute(:"skincondition") %>
                  |
              <% end %>
          <% end %>
          <% unless opdrecord.nails.blank? %>
              <% unless opdrecord.nails == "Normal" %>
                  <b>Nails :</b>
                  <%= opdrecord.read_attribute(:"nails") %>
                  |
              <% end %>
          <% end %>
          <% unless opdrecord.examinationsigns.blank? %>

              <%= opdrecord.read_attribute(:"examinationsigns") %>
              |
          <% end %>




          <div class="mb3"></div>
          <% if romcounter > 0 %>
              <b>ROM :</b>
          <%end%>

          <% if opdrecord.romsliderrangerightwristextnflexn.length > 0 && opdrecord.romsliderrangerightwristextnflexn != "-70 : 80" %>

              <b>Right Wrist:Extn-Flexn :</b>
              <%= opdrecord.read_attribute(:"romsliderrangerightwristextnflexn") %>
              <%= opdrecord.read_attribute(:"r_romwristextnflexn") %>
              <b>,</b>

          <% end %>
          <% if opdrecord.romsliderrangeleftwristextnflexn.length > 0 && opdrecord.romsliderrangeleftwristextnflexn != "-70 : 80" %>
              <b>Left Wrist:Extn-Flexn :</b>
              <%= opdrecord.read_attribute(:"romsliderrangeleftwristextnflexn") %>
              <%= opdrecord.read_attribute(:"l_romwristextnflexn") %>
              ,
          <% end %>



          <% if opdrecord.romsliderrangerightwristradioulnardev.length > 0 && opdrecord.romsliderrangerightwristradioulnardev != "-20 : 40" %>
              <b>Right Wrist:Radio-Ulnar Dev. :</b>
              <%= opdrecord.read_attribute(:"romsliderrangerightwristradioulnardev") %>
              <%= opdrecord.read_attribute(:"r_romwristradioulnardev") %>
              ,
          <% end %>
          <% if opdrecord.romsliderrangeleftwristradioulnardev.length > 0 && opdrecord.romsliderrangeleftwristradioulnardev != "-20 : 40" %>
              <b>Left  Wrist:Radio-Ulnar Dev. :</b>
              <%= opdrecord.read_attribute(:"romsliderrangeleftwristradioulnardev") %>
              <%= opdrecord.read_attribute(:"l_romwristradioulnardev") %>

          <% end %>

          <% if opdrecord.romsliderrangeright1stcmcabduction.length > 0 && opdrecord.romsliderrangeright1stcmcabduction != "0 : 70" %>
              <b>Right 1st CMC:Abduction:</b>
              <%= opdrecord.read_attribute(:"romsliderrangeright1stcmcabduction") %>
              <%= opdrecord.read_attribute(:"r_rom1stcmcabduction") %>
              ,
          <% end %>
          <% if opdrecord.romsliderrangeleft1stcmcabduction.length > 0 && opdrecord.romsliderrangeleft1stcmcabduction != "0 : 70" %>
