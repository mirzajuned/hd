              <th class="text-center" width="25%;"><%= clinical.input :airway_assesment, wrapper_html: {:class=>"primary_assesment_rb"}, collection: ['Normal', 'Abnormal'], :as => :hg_radio_button,:label => false, :required => false, :class => 'btn btn-xs btn-white', style: "margin-top:-2px;", :input_html => {:name => "inpatient_ipd_record[clinical_note_attributes][airway_assesment]" }, :checked => @clinical_note.read_attribute(:airway_assesment) || 'Normal' %></th>
