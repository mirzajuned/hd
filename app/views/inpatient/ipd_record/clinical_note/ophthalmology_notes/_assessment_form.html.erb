</style>

<script type="text/javascript">
  $(".primary_assesment_rb").on('change', function(){
    if($(this).find('.btn-brown').text() == "Abnormal") {
      $(this).closest('th').siblings('.toggle-head').find('.primary_assesment_cb').show();
      $(this).closest('th').siblings('.toggle-head').find('.click-options').hide();
    } else {
      $(this).closest('th').siblings('.toggle-head').find('.primary_assesment_cb').hide();
      $(this).closest('th').siblings('.toggle-head').find('.click-options').show();
    }
  })
  $('.primary_assesment_rb').trigger('change');
</script>