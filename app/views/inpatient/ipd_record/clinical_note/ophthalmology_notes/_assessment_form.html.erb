<% action = (params[:action] == "new") %>
<%= f.simple_fields_for @clinical_note do |clinical| %>
    <div class="row mb10" style="margin-top:10px;">
      <div class="col-md-12 text-center" style="text-decoration:underline;">
        PRIMARY ASSESSMENT
      </div>
    </div>
    <div class="row">
      <div class="col-md-8">
        <table class="table table-bordered primary-assesment">
          <tbody>
            <tr>
