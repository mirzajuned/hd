var patient_gender = "<%=@insurance_form.try(:gender)%>";
if(patient_gender == "Male"){
  $('#gender_male').trigger('click')
} else if(patient_gender == "Female"){
  $('#gender_female').trigger('click')
} else if(patient_gender == "Transgender"){
  $('#gender_transgender').trigger('click')
}

var current_insurance_status = "<%=@insurance_form.try(:is_currently_insured)%>";
if (current_insurance_status == "true"){
  $('#is_currently_insured_true').trigger('click')
}

