jQuery("#mail-record-modal").html('<%= j render "edit" %>')

var patient_mails = "<%= @email_tracker.receiver_details['patient_email'] %>".split(",");
$(".patient_email").email_multiple({
  data: patient_mails,
  container: 'email-receiver',
  element: 'email-receiver-input'
});

var cc_mails = "<%= @email_tracker.selectedtagnames %>".split(",");
$(".email_tracker_cc_mail").email_multiple({
  data: cc_mails,
  container: 'email-cc-receiver',
  element: 'email-cc-receiver-input'
});