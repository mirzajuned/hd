jQuery(".analyticscontentpanel").html("<%= escape_javascript(render partial: 'index.html') %>")

$(".btn-show-dashboard-form").on('click', function(e){
  e.preventDefault();
  $(".custom-dashboard-form").show()
  $(this).hide()
  $(".btn-hide-dashboard-form").parent().show()
})
$(".btn-hide-dashboard-form").on('click', function(e){
  e.preventDefault();
  $(".custom-dashboard-form").hide()
  $(this).hide()
  $(".btn-show-dashboard-form").parent().show()
})