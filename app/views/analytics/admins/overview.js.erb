$(".analyticscontentpanel").html("<%= escape_javascript(render partial: 'overview') %>")

  (function(){
     jQuery('.leftpanel').css({height: ''});
      var docHeight = jQuery(document).height();
      if(docHeight > jQuery('.mainpanel').height()){
        jQuery('.leftpanel').height(docHeight);
      }


  //  general analytics sparkline js
    jQuery('#weekly-appointment-created-counts').sparkline(<%= raw @total_appointment_created %>, {
      type: 'bar',
      height:'30px',
      barColor: '#428BCA'
    });

    jQuery('#weekly-appointment-arrived-counts').sparkline(<%= raw @total_appointment_arrived %>, {
      type: 'bar',
      height:'30px',
      barColor: '#428BCA'
    });

    jQuery('#weekly-admission-counts').sparkline(<%= raw @total_admission_created %>, {
      type: 'bar',
      height:'30px',
      barColor: '#428BCA'
    });


    // immediately invoked function end 
  }());
