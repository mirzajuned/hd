$("#pre_admission_special_note").html("<%= escape_javascript(render 'index') %>")

<%= render partial: "pre_admission_notes_init.js.erb" %>

function toggle_add_button(){
  if (parseInt("<%= @all_pre_admission_special_notes_count %>") < 1 ) {
    $('.pre_admission_special_note_add_button').show()
  } else {
    $('.pre_admission_special_note_add_button').hide()
  }
}
toggle_add_button()