<p id="notice"><%= notice %></p>

<h1>Listing Inventory Department Item Lots</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventory_department_item_lots.each do |inventory_department_item_lot| %>
      <tr>
        <td><%= link_to 'Show', inventory_department_item_lot %></td>
        <td><%= link_to 'Edit', edit_inventory_department_item_lot_path(inventory_department_item_lot) %></td>
        <td><%= link_to 'Destroy', inventory_department_item_lot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lot', new_inventory_department_item_lot_path %>
