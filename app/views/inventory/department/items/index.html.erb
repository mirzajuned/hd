<p id="notice"><%= notice %></p>

<h1>Listing Inventory Department Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventory_department_items.each do |inventory_department_item| %>
      <tr>
        <td><%= link_to 'Show', inventory_department_item %></td>
        <td><%= link_to 'Edit', edit_inventory_department_item_path(inventory_department_item) %></td>
        <td><%= link_to 'Destroy', inventory_department_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_inventory_department_item_path %>
