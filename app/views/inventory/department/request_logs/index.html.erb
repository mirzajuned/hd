<p id="notice"><%= notice %></p>

<h1>Listing Inventory Department Request Logs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventory_department_request_logs.each do |inventory_department_request_log| %>
      <tr>
        <td><%= link_to 'Show', inventory_department_request_log %></td>
        <td><%= link_to 'Edit', edit_inventory_department_request_log_path(inventory_department_request_log) %></td>
        <td><%= link_to 'Destroy', inventory_department_request_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request log', new_inventory_department_request_log_path %>
