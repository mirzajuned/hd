$("#inventory-modal")
  .html("<%= escape_javascript(render '/inventory/order/requisition_received/new_transfer.html') %>")
  .modal("show")

<%= render partial: 'inventory/transaction/transfers/new.js.erb' %>
<%= render partial: 'inventory/transaction/transfers/form.js.erb' %>

function variantLots(item_id) {
  $.ajax({
    type : 'get',
    url: '/inventory/order/requisition_received/variant_lots',
    data: {
      item_id: item_id,
      store_id: "<%= @inventory_store.id %>",
      id: "<%= @requisition_received.id %>",
      remote: "true"
    }
  })
}
