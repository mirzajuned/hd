<div class="row">
  <div class="col-sm-12 text-center" style="border-bottom:1px solid #ccc;border-top:1px solid #ccc;"><strong>Store Information</strong></div>
</div>
<div class="row" style="margin-top:15px;">
  <div class="col-sm-6">
    <strong>Name</strong><br>
    <%= f.text_field :name, { class: 'form-control p7_10 fullname', placeholder: "Fullname" } %>
  </div>
  <div class="col-sm-3">
    <strong>Abbreviation</strong><br>
    <%= f.text_field :abbreviation_name, { class: 'form-control p7_10 abbreviation', placeholder: "Abbreviation" , minlength: 3, maxlength: 10 } %>
  </div>
  <div class="col-sm-3">
    <strong>Store Type</strong><br>
    <% if @store_type == 'central_hub' %>
      <%= f.select :department_id, [["Central Hub", "550368792"]], { include_blank: true }, { class: 'form-control gender' } %>
      <%= f.hidden_field :store_type, { value: 'central_hub' } %>
    <% else %>
      <%= f.hidden_field :facility_name, { value: current_facility.name } %>
      <%= f.select :department_id, [["Pharmacy","284748001"],["Optical","50121007"],["Central","334046963"], ['OT Store', '384748001'], ['House Keeping', '384748004'], ['Maintenance', '384748005'], ['Marketing Store', '384748002'], ['Stationery Store', '384748003']], { include_blank: true }, { class: 'form-control gender' } %>
    <% end %>

    <%= f.hidden_field :department_name %>
  </div>

</div>

