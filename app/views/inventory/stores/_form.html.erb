    <div class="<%=@col_length%>">
      <div class="row">
        <div class="col-sm-12">
          <strong>Facilities</strong><br>
          <% facilities = (@facilities  if @facilities.present?) || [] %>
          <%= select_tag "inventory_store[facility_id]", options_for_select(facilities, @store.facility_id.to_s), { class: 'form-control facility_id', multiple: false } %>
        </div>
      </div>
    </div>
    <div class="<%=@col_length%> auto-requisition-central-hub">
      <div class="row">
        <div class="col-sm-12">
          <strong>Receiving Store</strong><br>
          <% requisition_requested_store = (@requisition_requested_store  if @requisition_requested_store.present?) || [] %>
          <%= select_tag "inventory_store[requisition_requested_store_id]", options_for_select(requisition_requested_store, @store.requisition_requested_store_id.to_s), { class: 'form-control requisition_requested_store_id', multiple: false, include_blank: true, required: true } %>
          <%= f.hidden_field :requisition_requested_store_name %>
        </div>
      </div>
    </div>
  <% end %>
  <% if @enable_entity_group %>
    <div class="col-sm-4">
      <strong>Entity Group</strong>
      <%= f.select :entity_group_id, options_for_select(@entity_groups.collect {|r| [ "#{r.name} - #{r.abbreviation}", r.id ] }, @store.entity_group_id), { include_blank: 'Select Entity Group'}, {class: 'form-control', style:'max-height:34px;'} %>
    </div>
  <% end %>
  <div class="col-sm-4">
    <strong>Shop Name</strong>
    <%= f.text_field :shop_name, { class: 'form-control p7_10 shop_name', placeholder: 'Shop Name' } %>
  </div>
</div>

