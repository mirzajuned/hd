$("#store-modal").html("<%= escape_javascript(render 'link_unlink_multiple_category.html.erb') %>")

$("#category_ids").select2()

function link_unlink_category() {

  $("#save_link_unlink_multiple_category").validate({
    rules: {
      "category_ids[]": {
        required: true,
      },
    },
    messages: {
      "category_ids[]": {
        required: "Select Atleast One Category"
      }
    },
  });
}
link_unlink_category()