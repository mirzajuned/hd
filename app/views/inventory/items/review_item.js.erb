jQuery("#inventory-modal").html("<%= escape_javascript(render  'inventory/items/item_review/optical/contact_review.html.erb') %>")
<% elsif @inventory_params["item_type"] == "other" %>
jQuery("#inventory-modal").html("<%= escape_javascript(render  'inventory/items/item_review/optical/other_review.html.erb') %>")
