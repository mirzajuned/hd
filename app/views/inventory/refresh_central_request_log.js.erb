jQuery("#items_request").html("<%= escape_javascript(render partial: 'inventory/refresh_central_request_log', locals: {inventory_department_request_logs: @inventory_department_request_logs }) %>")


$('.log-request').on('click', function(){

  request_log_id = $(this).attr('data-modelid')
  url = '/inventory/department/request_logs/'+request_log_id
  data = {
    department_id:  'central',
//      id: request_log_id,
  }

  $.ajax({
    type: "get",
    data: data,
    url: url,
  })
})