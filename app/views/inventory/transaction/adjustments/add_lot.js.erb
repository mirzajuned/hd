var class_val = '<%= params[:class_val] %>'
if (class_val != '') {
	$('.'+class_val).remove()
}

var id = '<%= params[:lot_id] %>'
$('#'+id).css('background-color', '#cfe8d2')
$('#'+id).addClass('disabled_row')

$('.transaction-table-body').append($("<%= escape_javascript(render partial: 'inventory/transaction/adjustments/add_lot.html.erb', locals: { adjust_transaction: @adjust_transaction } ) %>").html());

$("#add_lot_modal").modal("hide")
$("#add-unit-lot-modal").modal("hide")

<%= render partial: 'form.js.erb' %>
