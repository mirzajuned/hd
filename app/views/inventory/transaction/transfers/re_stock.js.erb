jQuery(".inventory-show-panel").html("<%= escape_javascript(render partial: 'inventory/transaction/transfers/show.html') %>")
$("#<%= @transfer_transaction.id %>.inventory-list-row")
  .html("<%=escape_javascript(render('transfer', transfer: @transfer_transaction))%>")
  .trigger('click')
$(".re-stock").remove();
<% if flash[:success].present? %>
  notice = new PNotify({ title: 'Re-Stocked', text: 'Transfer has been re-stocked', type: 'success' });
<% else %>
  notice = new PNotify({ title: 'Error', text: 'Please try again', type: 'error' });
<% end %>
notice.get().click(function(){ notice.remove() });