$("#inventory-modal").modal("hide")
$("#<%= @store_consumption.id %>.inventory-list-row")
	.html("<%=escape_javascript(render('store_consumption', store_consumption: @store_consumption))%>")
	.trigger('click')

<% if flash[:success].present? %>
  notice = new PNotify({ title: 'Updated', text: 'Consumption has been updated successfully', type: 'success' });
<% else %>
  notice = new PNotify({ title: 'Error', text: 'Please try again', type: 'error' });
<% end %>
notice.get().click(function(){ notice.remove() });