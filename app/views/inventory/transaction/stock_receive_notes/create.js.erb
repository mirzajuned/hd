<% if @stock_receive_note.new_record? %>
  notice = new PNotify({ title: 'Failed', text: 'SRN creation failed', type: 'error' });
<% else %>
  $("#inventory-modal").modal('hide')
  notice = new PNotify({ title: 'Created', text: 'New SRN created', type: 'success' });
<% end %>
notice.get().click(function(){ notice.remove() });

query = $("#inventory_search").val() || ""
filter_index(query)