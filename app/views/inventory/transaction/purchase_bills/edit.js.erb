<%= render partial: 'new.js.erb' %>
$(".transaction-list-row").each(function(){
  transfer_id = $(this).attr('data-id')
  _this_parent = $(this)
  $('.purchase-transaction').each(function(){
    id = $(this).attr('data')
    if (transfer_id == id){
      $(_this_parent).css('background-color', '#cfe8d2')
          $(_this_parent).addClass('disabled_row')
    }
  })
})