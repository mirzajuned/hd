$("#category-modal").html("<%= escape_javascript(render 'link_unlink_multiple_dispensing_unit.html.erb') %>")

$("#dispensing_unit_ids").select2()

function link_unlink_dispensing_unit() {

  $("#save_link_unlink_multiple_dispensing_unit").validate({
    rules: {
      "dispensing_unit_ids[]": {
        required: true,
      },
    },
    messages: {
      "dispensing_unit_ids[]": {
        required: "Select Atleast One Dispensing Unit"
      }
    },
  });
}
link_unlink_dispensing_unit()