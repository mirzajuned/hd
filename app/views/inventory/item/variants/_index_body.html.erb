<% @variants.each do |variant| %>
  <% if variant.empty %>
    <tr id="<%= variant.id%>" class="inventory-list-row variant-empty" style="background-color: #FFB6C1;" title="Item Empty">
  <% elsif variant.running_low %>
    <tr id="<%= variant.id%>" class="inventory-list-row variant-running-low" style="background-color: #FFA500;" title="Item Running Low">
  <% else %>
    <tr id="<%= variant.id%>" class="inventory-list-row variant-stock" style="background-color: #FFFAFA;" title="Total Stock&nbsp;&nbsp;<%= variant.stock %>">
  <% end %>
    <td style="font-size: 10px; color: #17a08c;" class="col-md-3"><%= variant.variant_code %></td>
    <td class="col-md-5 description"><b><%= variant.description %> <%= variant.variant_identifier %></b></td>

    <td class="col-md-2"><% if variant.stock %><%= variant.stock&.round(2) %><% else %>NA<% end %></td>
    <% if @inventory_store.department_id == '50121007' %>
      <td class="col-md-2"><% if variant&.model_no.present? %><%= variant.model_no %><% else %>NA<% end %></td>
    <% end %>
    <td class="col-md-2"><% if variant.category.present? %><%= variant.category&.titleize %><% else %>NA<% end %></td>
  </tr>
<%end%>