<%= javascript_include_tag "application-loggedin", "data-turbolinks-track" => true %>
<div class="modal-dialog contact-dialog" style="width:50%;">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal">&times;</button>
      <h4 class="modal-title">Edit Contact Form</h4>
    </div>
    <%= form_for @facility_contact, method: "patch", remote: true do |f| %>
      <%= f.hidden_field :organisation_id, value: @facility_contact.organisation_id.to_s %>
      <%= f.hidden_field :facility_id, value: @facility_contact.facility_id.to_s %>
      <%= f.hidden_field :is_updated, value: true %>
      <%= render partial: 'form.html.erb', locals: { f: f } %>
    <% end %>
  </div>
</div>