$("#appointment-modal").html("<%= escape_javascript(render 'form') %>")

function validate_patient_tracker(){
  $("#patient-tracker-form").validate({
    rules: {
      "patient_tracker[count]": {
        required: true,
      },
      "patient_tracker[description]": {
        required: true,
      }
    },
    messages: {
      "patient_tracker[count]": {
        required: "Cannot be Blank"
      },
      "patient_tracker[description]": {
        required: "Description Cannot be Blank",
      }
    },
    errorPlacement: function (error, element) {
      error.insertAfter(element);
    }
  })
}