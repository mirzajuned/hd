jQuery("#insurance-modal").html("<%= escape_javascript(render 'show') %>")
jQuery("#insurance-modal").modal("show")

// Admission Management
if($(".admission-panel").length > 0){
  $(".admission_today").find('.active').find("[data-admission-id='<%= @copay.admission_id.to_s %>']").trigger('click');
}

// OT Management
if($(".ot-panel").length > 0){
  $(".ot_today").find('.active').find("[data-ot-id=" + $("#ot_refresher").val() + "]").trigger('click');
}