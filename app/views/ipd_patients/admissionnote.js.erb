$("#inpatient-modal").html("<%= escape_javascript(render 'admissionnote') %>")
$("#inpatient-modal").modal("show")
jQuery('.inpatient-admission-note-summary').on("click", function() {
    var params = {admissionid: jQuery(this).data("admissionid") }
    jQuery.ajax({
        type: "get",
        dataType: "script",
        url: "/ipd_patients/admissionsummary",
        data: {admission: params, remote: "true", 'toggle': "modal", 'target': '#inpatient-modal'}
    })
})
jQuery('.admission-note-print').on("click", function() {
	var params = {admissionid: jQuery(this).data("admissionid") }
    jQuery.ajax({
        type: "get",
        dataType: "script",
        url: "/ipd_patients/admissionsummary",
        data: {admission: params, remote: "true", 'toggle': "modal", 'target': '_blank', 'format':'pdf'}
    })
})