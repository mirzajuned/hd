jQuery(".admissions-list").html("<%= escape_javascript(render 'ipd_patients/admissionslist') %>")
jQuery("#inpatient-modal").modal("hide")
jQuery('.inpatient-controls-make-admission').on("click", function() {
    jQuery.ajax({
        type: "get",
        dataType: "script",
        url: "/ipd_patients/makeadmission",
        data: {remote: "true", 'toggle': "modal", 'target': '#inpatient-modal'}
    })
})
jQuery('.inpatient-controls-print-admission-list').on("click", function() {
