        data: {remote: "true", 'toggle': "modal", 'target': '#inpatient-modal'}
    })
})
jQuery('.inpatient-admission-discharge-update').on("click", function() {
    var params = {patientid: jQuery(this).data("patientid"), admissionid: jQuery(this).data("admissionid") }
    jQuery.ajax({
        type: "get",
        dataType: "script",
        url: "/ipd_patients/updatedischarge",
        data: {admission: params, remote: "true", 'toggle': "modal", 'target': '#inpatient-modal'}
