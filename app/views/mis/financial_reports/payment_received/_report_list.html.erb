        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Withholding Tax</span>
      <% end %>
    </li>
    <li data-test-title="Sales by Payer">
      <%= link_to reports_view_mis_financial_reports_path(group: group, title: "sales_by_payer"), data: { remote: true }, class: "mis_reports_link ps" do %>
