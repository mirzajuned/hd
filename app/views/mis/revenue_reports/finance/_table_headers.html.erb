<thead>
<% if title == 'billing_statistics'%>
  <tr>
    <th class="text-center report-table-header" rowspan="2">Date</th>
    <th class="text-center report-table-header" rowspan="2">Department</th>
    <th class="text-center report-table-header" colspan="6">All Bills</th>
    <th class="text-center report-table-header" colspan="5">Cash Bills</th>
    <th class="text-center report-table-header" colspan="6">Credit Bills</th>
  </tr>
  <tr>
    <th class="text-center"># Of Bills</th>
    <th class="text-center">Gross Amount</th>
    <th class="text-center">Net Amount</th>
    <th class="text-center">Refunded Amount</th>
    <th class="text-center">Offer</th>
    <th class="text-center">Discount</th>
    <th class="text-center"># Of Bills</th>
    <th class="text-center">Received </th>
    <th class="text-center">Refunded Amount</th>
    <th class="text-center">Offer </th>
    <th class="text-center">Discount </th>
    <th class="text-center"># Of Bills</th>
    <th class="text-center">Received</th>
    <th class="text-center">Refunded Amount</th>
    <th class="text-center">Offer</th>
    <th class="text-center">Discount</th>
    <th class="text-center">Pending</th>
  </tr>
<% elsif title == 'collection_summary'%>
  <tr>
    <th class="text-center report-table-header" rowspan="2">Date</th>
    <th class="text-center report-table-header" rowspan="2">Department</th>
    <th class="text-center report-table-header" rowspan="2">Net Collection</th>
    <th class="text-center report-table-header" colspan="4">Collection/Cash Inflow</th>
    <th class="text-center report-table-header" colspan="3">Receivables/To be received</th>
    <th class="text-center report-table-header" colspan="4">Cancel/Refund</th>
  </tr>
  <tr>
    <th class="text-center">Cash Sale</th>
    <th class="text-center">Cash Advance</th>
    <th class="text-center">Credit Sale Settled/Received</th>
    <th class="text-center">Total Collection</th>
    <th class="text-center">Credit Sale(Self)</th>
    <th class="text-center">Credit Sale(Third party)</th>
    <th class="text-center">Total Receivables</th>
    <th class="text-center">Cash Sale Refund</th>
    <th class="text-center">Credit Sale Refund</th>
    <th class="text-center">Advance Refund</th>
    <th class="text-center">Total Sale Refund</th>
  </tr>
<% elsif title == 'bills_by_payer'%>
  <tr>
    <th class="text-center report-table-header" rowspan="2">Name</th>
    <th class="text-center report-table-header" rowspan="2"># Of Bills</th>
    <th class="text-center report-table-header" rowspan="2">Total Revenue</th>
    <th class="text-center report-table-header" rowspan="2">Amount Pending</th>
    <th class="text-center report-table-header" colspan="2">1-15 Days</th>
    <th class="text-center report-table-header" colspan="2">16-30 Days</th>
    <th class="text-center report-table-header" colspan="2">31-45 Days</th>
    <th class="text-center report-table-header" colspan="2">46-60 Days</th>
    <th class="text-center report-table-header" colspan="2">More than 60 Days</th>
  </tr>
  <tr>
    <th class="text-center"># Of Bills</th>
    <th class="text-center">Amount Pending</th>
    <th class="text-center"># Of Bills</th>
    <th class="text-center">Amount Pending </th>
    <th class="text-center"># Of Bills</th>
    <th class="text-center">Amount Pending</th>
    <th class="text-center"># Of Bills</th>
    <th class="text-center">Amount Pending</th>
    <th class="text-center"># Of Bills</th>
    <th class="text-center">Amount Pending</th>
  </tr>
<% elsif title == 'bills_by_package' %>
  <% title_postfix = title.split('_').last %>
  <tr>
    <th class="text-center" rowspan="2">&nbsp;<%=title_postfix.titleize%>&nbsp;Billed&nbsp;On&nbsp;</th>
    <th id="<%="th_sub_specialty_#{title_postfix}"%>" class="text-center" rowspan="2">&nbsp;Sub&#45;Specialty&nbsp;</th>
    <th class="text-center" rowspan="2">&nbsp;Department&nbsp;</th>
    <th class="text-center" rowspan="2">&nbsp;<%=title_postfix.titleize%>&nbsp;Name&nbsp;</th>
    <th class="text-center" colspan="2">&nbsp;All&nbsp;Bills&nbsp;</th>
    <th class="text-center" colspan="2">&nbsp;Free&nbsp;Bills&nbsp;</th>
    <th class="text-center" colspan="2">&nbsp;Paid&nbsp;Bills&nbsp;</th>
    <th class="text-center" colspan="2">&nbsp;Discounted&nbsp;Bills&nbsp;</th>
  </tr>
  <tr>
    <th class="text-center">&nbsp;Total&nbsp;Billed&nbsp;(#)&nbsp;</th>
    <th class="text-center">&nbsp;Gross&nbsp;Amount&nbsp;</th>
    <th class="text-center">&nbsp;Free&nbsp;Billed&nbsp;(#)&nbsp;</th>
    <th class="text-center">&nbsp;Gross&nbsp;Collected&nbsp;</th>
    <th class="text-center">&nbsp;Paid&nbsp;Billed&nbsp;(#)&nbsp;</th>
    <th class="text-center">&nbsp;Gross&nbsp;Collected&nbsp;</th>
    <th class="text-center">&nbsp;Discounted&nbsp;Billed&nbsp;(#)&nbsp;</th>
    <th class="text-center">&nbsp;Gross&nbsp;Collected&nbsp;</th>
  </tr>
<!-- TODO: bills_by_package and bills_by_services should come together, once offer is implemented for packages  -->
<% elsif title == 'bills_by_services'%>
  <% title_postfix = title.split('_').last %>
  <tr>
    <th class="text-center" rowspan="2">&nbsp;<%=title_postfix.titleize%>&nbsp;Billed&nbsp;On&nbsp;</th>
    <th id="<%="th_sub_specialty_#{title_postfix}"%>" class="text-center" rowspan="2">&nbsp;Sub&#45;Specialty&nbsp;</th>
    <th class="text-center" rowspan="2">&nbsp;Department&nbsp;</th>
    <th class="text-center" rowspan="2">&nbsp;<%=title_postfix.titleize%>&nbsp;Name&nbsp;</th>
    <th class="text-center" colspan="2">&nbsp;All&nbsp;Bills&nbsp;</th>
    <th class="text-center" colspan="2">&nbsp;Free&nbsp;Bills&nbsp;</th>
    <th class="text-center" colspan="2">&nbsp;Paid&nbsp;Bills&nbsp;</th>
    <th class="text-center" colspan="2">&nbsp;Bills&nbsp;with&nbsp;Discount&nbsp;and&nbsp;Offers&nbsp;</th>
  </tr>
  <tr>
    <th class="text-center">&nbsp;Total&nbsp;Billed&nbsp;(#)&nbsp;</th>
    <th class="text-center">&nbsp;Gross&nbsp;Amount&nbsp;</th>
    <th class="text-center">&nbsp;Free&nbsp;Billed&nbsp;(#)&nbsp;</th>
    <th class="text-center">&nbsp;Gross&nbsp;Collected&nbsp;</th>
    <th class="text-center">&nbsp;Paid&nbsp;Billed&nbsp;(#)&nbsp;</th>
    <th class="text-center">&nbsp;Gross&nbsp;Collected&nbsp;</th>
    <th class="text-center">&nbsp;Bills&nbsp;with&nbsp;offers&nbsp;and&nbsp;Discounts&nbsp;(#)&nbsp;</th>
    <th class="text-center">&nbsp;Gross&nbsp;Collected&nbsp;</th>
  </tr>
<% elsif title == 'collection_by_user'%>
  <tr>
    <th class="text-center report-table-header" rowspan="2">Date</th>
    <th class="text-center report-table-header" rowspan="2">User Name</th>
    <th class="text-center report-table-header" rowspan="2">Department Name</th>
    <th class="text-center report-table-header" rowspan="2">Net Collection</th>
    <th class="text-center report-table-header" colspan="2">Advances</th>
    <th class="text-center report-table-header" colspan="2">Bills</th>
    <th class="text-center report-table-header" colspan="2">Total Collection</th>
    <th class="text-center report-table-header" colspan="2">Cancel/Refund</th>
  </tr>
  <tr>
    <th class="text-center"># Of Receipts</th>
    <th class="text-center">Amount</th>
    <th class="text-center"># Of Receipts</th>
    <th class="text-center">Amount</th>
    <th class="text-center"># Of Receipts</th>
    <th class="text-center">Amount</th>
    <th class="text-center"># Of Receipts</th>
    <th class="text-center">Amount</th>
  </tr>  
<% elsif title == 'pharmacy_conversion_stats' || title == 'optical_conversion_stats' %>
  <tr>
    <th class="text-center">Date</th>
    <th class="text-center">Total Advised</th>
    <th class="text-center">Total Converted</th>
    <th class="text-center">Conversion (in %)</th>
  </tr>
<% elsif title == 'procedure_bill_type_stats' %>
  <tr>
    <th class="text-center">Procedure Name</th>
    <th class="text-center">Type</th>
    <th class="text-center">Gender</th>
    <th class="text-center">0-15 (Child)</th>
    <th class="text-center">16-55 (Adult)</th>
    <th class="text-center">55+ (Elderly)</th>
    <th class="text-center">Unspecified</th>
    <th class="text-center">Total</th>
  </tr>
<% elsif title == 'optical_bill_type_stats' %>
  <tr>
    <th class="text-center">Type</th>
    <th class="text-center">Gender</th>
    <th class="text-center">0-15 (Child)</th>
    <th class="text-center">16-55 (Adult)</th>
    <th class="text-center">55+ (Elderly)</th>
    <th class="text-center">Unspecified</th>
    <th class="text-center">Total</th>
  </tr>
<% end %>
</thead>

