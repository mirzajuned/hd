// MIS List Search & Select
$("#revenue_mis_lists").select2({matcher: modelMatcher})

$("#revenue_mis_lists").on("change", function(e){
  e.preventDefault()
  var url = '/mis/revenue_reports/reports_view'
  var title = $(this).val();
  var group = $(this).find('option:selected').parent().attr('label').toLowerCase();

  $.ajax({
    type: "GET",
    dataType: "script",
    url: url,
    data: {
      title: title,
      group: group
    }
  });

  pushState_url = `${url}?group=${group}&title=${title}`
  history.pushState('', '', pushState_url); // history.pushState([data], [title], [url]);
});
