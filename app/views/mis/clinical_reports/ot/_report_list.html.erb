<div class="mis_list">
  <h4>OT List</h4>
  <% group = "ot" %>
  <ul data-test-title="Conversion" class="nav">
    <li data-test-title="Date Wise">
      <%= link_to reports_view_mis_clinical_reports_path(group: group, title: "ot_date_wise"), data: { remote: true }, class: "mis_reports_link ps" do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Date Wise</span>
      <% end %>
    </li>
    <li data-test-title="Facility Wise">
      <%= link_to reports_view_mis_clinical_reports_path(group: group, title: "ot_facility_wise"), data: { remote: true }, class: "mis_reports_link ps" do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Facility Wise</span>
      <% end %>
    </li>
    <li data-test-title="Doctor Wise">
      <%= link_to reports_view_mis_clinical_reports_path(group: group, title: "ot_doctor_wise"), data: { remote: true }, class: "mis_reports_link ps" do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Doctor Wise</span>
      <% end %>
    </li>
    <li data-test-title="  List By Theatre">
      <a href="#" class="empty_response mis_reports_link">
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;List By Theatre</span>
        <span class="mis-coming-soon">&emsp;Coming Soon</span>
      </a>
    </li>
  </ul>
</div>
