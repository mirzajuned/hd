var reports = jQuery('#procedure_daily_doctor_procedure').html("<%= escape_javascript(render partial: '/mis/new_clinical_reports/procedure/daily_doctor_procedure.html.erb') %>")
$('#procedure_daily_doctor_procedure').DataTable( {
  "sDom": "<'top'p>",
  "bLength": false,
  "bFilter": false,
  "bInfo": false,
  "bDestroy": true,
  "ordering": false,
  "sPaginationType": "full_numbers",
  "iDisplayStart": parseInt("<%= @mis_params[:iDisplayStart] || 0 %>"),
  "iDisplayLength": parseInt("<%= @mis_params[:iDisplayLength] || 100 %>"),
  "bServerSide": true,
  "sAjaxSource": "/mis/new_clinical_reports/reports_data.json?start_date=<%= @mis_params[:start_date] %>&end_date=<%= @mis_params[:end_date] %>&facility_id=<%= @mis_params[:facility_id] %>&facility_name=<%= @mis_params[:facility_name].gsub(/\'/,'`') %>&time_period=<%= @mis_params[:time_period] %>&title=<%= @mis_params[:title] %>&group=<%= @mis_params[:group] %>&initial_age=<%= @mis_params[:initial_age] %>&final_age=<%= @mis_params[:final_age] %>&gender_type=<%= @mis_params[:gender_type] %>&consulting_doctor=<%= @mis_params[:consulting_doctor] %>&admission_type=<%= @mis_params[:admission_type] %>&ipd_current_status=<%= @mis_params[:ipd_current_status] %>&date_wise=<%= @mis_params[:date_wise] %>&capture_state=<%= @mis_params[:capture_state] %>&moh=<%= @mis_params[:ipd_current_status] %>&moh=<%= @mis_params[:moh] %>&location_type=<%= @mis_params[:location_type] %>&loc_name=<%= @mis_params[:loc_name] %>&procedure_state=<%= @mis_params[:procedure_state] %>&procedure_date_wise=<%= @mis_params[:procedure_date_wise] %>&doctor_type=<%= @mis_params[:doctor_type] %>&has_params=true",
  "oLanguage": {
    "sZeroRecords": "Nothing to Display",
    "sLoadingRecords": "Please wait - loading reports..."
  },
  "rowCallback": function( row, data ) {
    $(row).find('td:eq(0)').addClass('align_left');
    $(row).find('td:eq(1)').addClass('align_left');
    $(row).find('td:eq(2)').addClass('align_left');
    $(row).find('td:eq(3)').addClass('align_left');
  }
} );

content = '<table class="no-border">'
content += '<tr><td class="div_clinical_header">Note: </td><td>This report shows # of advised and # performed procedures per day by individual doctors. Advised and performed are exclusive of each other.</td></tr>'
content += '<tr><td></td><td>A doctor may have advised no procedures for a day but still may have performed some (that may have been advised earlier by other doctors)</td></tr>'
content += '</table>'

$("#div_clinical_note").html(content);
$("#div_clinical_note").show();