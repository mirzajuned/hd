<div class="mis_list">
  <h3> Patient Referred to</h3>
  <% group = 'referral' %>
  <ul data-test-title="Outpatient" class="nav">
    <li data-test-title="Patient Referred to Summary">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'patient_referred_to_summary'), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Patient Referred to Summary</span>
      <% end %>
    </li>
    <li data-test-title="Patient Referred to Stats">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'patient_referred_to_stats'), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Patient Referred to Stats</span>
      <% end %>
    </li>
  </ul>
</div>