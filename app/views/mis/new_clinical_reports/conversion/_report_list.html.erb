<div class="mis_list">
  <h3> Inventory Reports</h3>
  <% group = 'conversion' %>
  <h4 style="padding-left: 10px"> Stats</h4>
  <ul data-test-title="Conversion Stats" class="nav">
    <li data-test-title="Pharmacy Conversion Stats">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'pharmacy_conversion_stats'), data: { remote: true }, class: "mis_reports_link ps" do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Pharmacy Conversion Stats</span>
      <% end %>
    </li>
    <li data-test-title="Optical Conversion Stats">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'optical_conversion_stats'), data: { remote: true }, class: "mis_reports_link ps" do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Optical Conversion Stats</span>
      <% end %>
    </li>
  </ul>
  <h4 style="padding-left: 10px"> Summary</h4>
  <ul data-test-title="Conversion Summary" class="nav">
    <li data-test-title="Pharmacy Conversion">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'pharmacy_conversion_summary'), data: { remote: true }, class: "mis_reports_link ps" do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Pharmacy Conversion Summary</span>
      <% end %>
    </li>
    <li data-test-title="Optical Conversion">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'optical_conversion_summary'), data: { remote: true }, class: "mis_reports_link ps" do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Optical Conversion Summary</span>
      <% end %>
    </li>
  </ul>
  
</div>