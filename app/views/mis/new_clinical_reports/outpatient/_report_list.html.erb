  <div class="mis_list">
  <h3>Outpatient</h3>
  <% group = 'outpatient' %>
  <ul data-test-title="Outpatient" class="nav">
    <li data-test-title="Patient Type Wise">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'patient_detail'), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Visit Summary</span>
      <% end %>
    </li>
    <li data-test-title="Date Wise">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'opd_statistics'), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Visit Statistics</span>
      <% end %>
    </li>
    <li data-test-title="Date Wise">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'location_statistics'), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Patient By Region (city, state, etc.)</span>
      <% end %>
    </li>

    <li data-test-title="Date Wise">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'referral_statistics'), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Referral Statistics</span>
      <% end %>
    </li>
    <li data-test-title="Patient Time Spent Type Wise">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'patient_time_spent_wise'), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Patient Time Spent</span>
      <% end %>
    </li>
    <li data-test-title="User Wise TAT Report">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'user_wise_tat', role_filter: true), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;User wise TAT</span>
      <% end %>
    </li>
    <li data-test-title="Role Wise TAT Report">
      <%= link_to reports_view_mis_new_clinical_reports_path(group: group, title: 'role_wise_tat'), data: { remote: true }, class: 'mis_reports_link ps' do %>
        <span class="mis-report-text"><i class="fa fa-arrow-right"></i>&emsp;Role wise TAT</span>
      <% end %>
    </li>
  </ul>
</div>
