<nav class="navbar navbar-inverse navbar-fixed-top mis_navbar">
  <%= render partial: "index_navbar.html.erb" %>
</nav>

<div class="mis-reports-list">
  <div class="row" style="padding:30px;">
    <div class="col-md-12">
      <div class="row">
        <div class="col-sm-4">
          <!-- Outpatient -->
          <%= render partial: "/mis/new_clinical_reports/outpatient/report_list.html.erb" %>
          <%= render partial: "/mis/new_clinical_reports/inpatient/report_list.html.erb" %>
          <%= render partial: "/mis/new_clinical_reports/diagnosis/report_list.html.erb" %>
          <%= render partial: "/mis/new_clinical_reports/referral/report_list.html.erb" %>
          <%= render partial: "/mis/new_clinical_reports/legacy_reports/report_list.html.erb" %>
        </div>
        <div class="col-sm-4">
          <%= render partial: "/mis/new_clinical_reports/procedure/report_list.html.erb" %>
          <%= render partial: "/mis/new_clinical_reports/conversion/report_list.html.erb" %>
        </div>
        <div class="col-sm-4">
          <%= render partial: "/mis/new_clinical_reports/investigation/report_list.html.erb" %>
          <%= render partial: "/mis/clinical_reports/ot/report_list.html.erb" %>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
  // Window Size
  var window_height = $(window).height() - 50
  $(".mis_reports_panel").css('height', window_height)

  $(window).resize(function() {
    var window_height = $(window).height() - 50
    $(".mis_reports_panel").css('height', window_height)
  })


  $(".empty_response").on('click', function(e){
    e.preventDefault();

    if ($('.ui-pnotify').length == 0) {
      notice = new PNotify({
        title: 'Coming Soon',
        text: 'Hold on tight, this report will be available soon',
        type: 'success'
      });
      notice.get().click(function(){ notice.remove() });
    }
  })
</script>
