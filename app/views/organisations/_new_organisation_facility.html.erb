<div class="row form-group">
  <div class="col-md-2"><%= f.label "Name :" %></div>
  <div class="col-md-5"><%= f.text_field :name,{:class => 'form-control'} %></div>
</div>
<div class="row form-group">
  <div class="col-md-2"><%= f.label "Telephone :" %></div>
  <div class="col-md-5"><%= f.text_field :telephone,{:class => 'form-control'} %></div>
</div>
<div class="row form-group">
  <div class="col-md-2"><%= f.label "email :" %></div>
  <div class="col-md-5"><%= f.text_field :email,{:class => 'form-control'} %></div>
</div>
<div class="row form-group">
  <div class="col-md-2"><%= f.label "City :" %></div>
  <div class="col-md-5"><%= f.text_field :city,{:class => 'form-control'} %></div>
</div>
<div class="row form-group">
  <div class="col-md-2"><%= f.label "PinCode :" %></div>
  <div class="col-md-5"><%= f.text_field :pincode,{:class => 'form-control'} %></div>
  <%= f.hidden_field :organisation_id,{:class => 'form-control'} %>
</div>
<div class="row form-group">
  <div class="col-md-2"></div>
  <div class="col-md-5"><%= f.submit "Save Changes",{:class => 'btn btn-success'} %></div>
</div>