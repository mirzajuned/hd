var target_id = "<%= escape_javascript(@target_id) %>"
var row_count = target_id.replace("<%= escape_javascript(@prefix) %>","")
	row_count = row_count.replace("_label","")

jQuery("#<%= escape_javascript(@prefix) %>"+row_count+"_service_id").val("<%= escape_javascript(@service[:id].to_s) %>")
jQuery("#<%= escape_javascript(@prefix) %>"+row_count+"_unit_price").val("<%= escape_javascript(@service[:unit_cost].to_s) %>")
jQuery("#<%= escape_javascript(@prefix) %>"+row_count+"_total_units").val("<%= escape_javascript(@service[:default_units].to_s) %>")
jQuery("#<%= escape_javascript(@prefix) %>"+row_count+"_total_units").change()