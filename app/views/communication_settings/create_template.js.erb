<% if flash[:error].present? %>
	$("#error_msg").addClass('text-danger')
	$("#error_msg").html("<%= flash[:error] %>")
<% else %>
	$("#communication_settings-table").html("<%= escape_javascript(render 'communication_settings/approved_templates/approved_templates.html.erb') %>")
	$("#communication-user-template-modal").modal("hide")
	<%= render partial: 'index_disable_toggle.js.erb' %>
<% end %>
<%= render partial: 'communication_settings/approved_templates/active.js.erb' %>
<%= render partial: 'communication_settings/approved_templates/disable.js.erb' %>
