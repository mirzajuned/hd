<% if flash[:error].present? %>
	$("#error_msg").addClass('text-danger')
	$("#error_msg").html("<%= flash[:error] %>")
<% else %>
	$("#communication_settings-table").html("<%= escape_javascript(render 'communication_settings/approved_templates/approved_templates.html.erb') %>")
	$("#communication-template-edit-modal").modal("hide")
<% end %>
