<% if flash[:error].present? %>
	$("#error_msg").addClass('text-danger')
	$("#error_msg").html("<%= flash[:error] %>")
<% else %>
	$("#communication_template_settings").html("<%= escape_javascript(render 'index.html.erb') %>")
	$("#communication-template-whatsapp-setting-modal").modal("hide")
	<%= render partial: 'index_disable_toggle.js.erb' %>
<% end %>