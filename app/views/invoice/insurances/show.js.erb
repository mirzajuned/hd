$("#insurance-modal").html("<%= escape_javascript(render 'invoice/insurances/show') %>")
$("#insurance-modal").modal("show")

<%= render :partial => 'invoice/insurances/show_view_calculations' %>

<% unless params[:action] == "Show" %>
  $("#ot-calendar").fullCalendar('refetchEvents');
<% end %>

<% unless params[:action] == "show" %>
  // Admission Management
  if($(".admission-panel").length > 0){
    $(".admission_today").find('.active').find("[data-admission-id='<%= @admission.id.to_s %>']").trigger('click');
  }

  // OT Management
  if($(".ot-panel").length > 0){
    $(".ot_today").find('.active').find("[data-ot-id=" + $("#ot_refresher").val() + "]").trigger('click');
  }

  if($(".patient-timeline-panel").length > 0){
    $("#btn-timeline-refresh").trigger('click')
  }
<% end %>