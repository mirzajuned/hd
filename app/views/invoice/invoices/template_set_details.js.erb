$(".template_set_details_options").html("<%= escape_javascript(render 'invoice/invoices/template_set_details.html') %>")


$('#template_set_details_option').on('click', function(){
  if (this.value != undefined && this.value != "" ) {
    $.ajax({
      type: "get",
      dataType: "script",
      url: "/invoice/invoices/append_template_set_details",
      data: { set_id: this.value, invoice_type: "<%= @invoice_type %>", department_id: "<%= @department_id %>",
        specialty_id: "<%= @specialty_id %>" ,appointment_id: '<%= @appointment_id.to_s %>',admission_id: '<%= @admission_id.to_s %>',
        invoice_id: '<%= @invoice_id.to_s %>', patient_id: '<%= @patient_id.to_s %>' }
    })
  }
})