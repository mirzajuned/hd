
$("#recounsel-<%= params[:order_advised_id] %>").closest(".orders-data").after($("<%= escape_javascript(render partial: '/orders/counselling_records/add_orders_data_form.html.erb') %>").html())
$("#recounsel-<%= params[:order_advised_id] %>").hide()

$(".remove-recounsel").on('click', function(){
  $(this).closest(".orders-data").remove()
  $("#recounsel-<%= params[:order_advised_id] %>").show()
})