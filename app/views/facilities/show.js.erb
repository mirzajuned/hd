$("#facility-modal").html("<%= escape_javascript(render 'show.html.erb') %>")

$('#manage_facilities').dataTable().fnDraw()

if ('<%= params[:facility_settings][:user_set_station] && params[:facility_settings][:queue_management] rescue false %>' == 'true') {
  $('#user-station-link').show();
} else {
  $('#user-station-link').hide();
}
