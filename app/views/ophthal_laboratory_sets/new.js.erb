$("#laboratory-set-modal").html("<%= escape_javascript(render 'form') %>")

function ophthal_set_form() {
  $("#new-ophthal-set-form").validate({
    rules: {
      "ophthal_laboratory_set[name]": {
        required: true,
      }
    },
    messages: {
      "ophthal_laboratory_set[name]": {
        required: "Set Name is Required"
      }
    },
    errorPlacement: function (error, element) {
      error.insertAfter(element);
    }
  });

  $.validator.addMethod("sRequired", $.validator.methods.required, "Side Required");
  $.validator.addClassRules("investigationside", { sRequired: true });
}
ophthal_set_form()