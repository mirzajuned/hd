if (jQuery(".medication-table-body-added > .treatmentmedications").length == 0){
    jQuery(".medication-table-body-added").append('<%= escape_javascript(render partial: "/inpatients/nursing_records/#{@speciality_folder_name}_notes/medication_partial.html.erb", locals: { i: @counter}) %>')
  
} else {
    jQuery(".medication-table-body-added > .treatmentmedications:last").after('<%= escape_javascript(render partial: "/inpatients/nursing_records/#{@speciality_folder_name}_notes/medication_partial.html.erb", locals: {i: @counter}) %>')
}