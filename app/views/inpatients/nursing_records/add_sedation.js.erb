if (jQuery(".sedation-table-body-added > .sedations").length == 0){
    jQuery(".sedation-table-body-added").append('<%= escape_javascript(render partial: "/inpatients/nursing_records/sedation_partial.html.erb", locals: { i: @counter}) %>')
  
} else {
    jQuery(".sedation-table-body-added > .sedations:last").after('<%= escape_javascript(render partial: "/inpatients/nursing_records/sedation_partial.html.erb", locals: {i: @counter}) %>')
}
