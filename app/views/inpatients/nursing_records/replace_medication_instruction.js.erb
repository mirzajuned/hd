$('.replace_medication').html("<%= escape_javascript(render partial: "/inpatients/nursing_records/#{@specality}_notes/replace_medication_instruction", locals:{nursingrecord: @nursing_record,advice_language: @advice_language, patient: @patient, specalityfoldername: @specality}) %>")
$('.multi-print').each(function(i, obj) {
  ex_quey = $(this).find('a').attr('href')
