<style>
  .ui-autocomplete {
    max-height: 150px;
    overflow-y: auto;
    /* prevent horizontal scrollbar */
    overflow-x: hidden;
    /* add padding to account for vertical scrollbar */
    background-color: #d9edf7;
  }
</style>

<% if @current_facility.show_language_support %>
