$('.tpa-corporate-details').prepend($("<%= escape_javascript(render partial: 'finance/tpa_masters/corporate_details', locals: {finance_tpa: @finance_tpa} ) %>").html());


$('.insurance-master-id').select2()
$('.corporate-master-id').select2()

$('.insurance-master-id').on('change', function () {
  var selected_text = $(this).find('option:selected').text()
  $(this).siblings('.insurance-master-name').val(selected_text)
})

$('.corporate-master-id').on('change', function () {
  var selected_text = $(this).find('option:selected').text()
  $(this).siblings('.corporate-master-name').val(selected_text)
})


$('.remove-tpa-corporate-detail').on('click',function () {
  $(this).parents('.tpa-corporate-detail').remove()
})