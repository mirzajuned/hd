
<%= f.fields_for :corporate_details, :child_index => index  do |cf| %>
  <table width="100%" class="tpa-corporate-detail">
    <tr valign="top">
      <td width="40%">
        <div class="form-group">
          <div class="col-sm-3 item-label"><%= cf.label :insurance %></div>
          <div class="col-sm-8">

            <%= cf.select :insurance_master_id, @finance_insurances.pluck(:name, :id),{ selected: cf.object.insurance_master_id, include_blank: true },{ class: "form-control p7_10 mb5 insurance-master-id" } %>


            <%= cf.hidden_field :insurance_master_name,{ :class=> 'form-control p7_10 mb5 insurance-master-name', :placeholder=>'' } %>
            <% unless cf.object.new_record? %>
              <%= cf.hidden_field :id %>
            <% end %>
          </div>
        </div>
      </td>
      <td>
        <%= cf.hidden_field :_destroy,  { value: 0 ,class: 'form-control' } %>
      </td>
      <td width="40%">
        <div class="form-group">
          <div class="col-sm-3 item-label"><%= cf.label :corporate %></div>
          <div class="col-sm-8 mb5 value-container">
            <%= cf.select :corporate_master_id, @finance_corporates.pluck(:name, :id),{ selected: cf.object.corporate_master_id, include_blank: true },{ class: "form-control p7_10 mb5 corporate-master-id" } %>

            <%= cf.hidden_field :corporate_master_name,{ :class=> 'form-control p7_10 mb5 corporate-master-name', :placeholder=>'' } %>

          </div>
        </div>
      </td>
      <% if f.object.new_record? %>
        <td width="10%">
          <button type="button" class="btn btn-danger btn-xs remove-tpa-corporate-detail">X</button>
        </td>
      <% else %>
        <td width="10%">
        </td>
      <% end %>
    </tr>
  </table>
<% end %>
