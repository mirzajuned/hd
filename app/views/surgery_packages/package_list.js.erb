$("#surgery_package_table_content").html("<%= j render partial: 'surgery_package_list.html.erb' %>")

// BACK Button
$(".back-btn").show()
$(".back-btn").on('click', function(e){
  e.preventDefault()
  let specialty_id = $("#manage_surgery_packages").find('.specialty_dropdown').val()
  $.ajax({
    type: "GET",
    dataType: "script",
    url: `/surgery_packages?specialty_id=${specialty_id}`
  });
  $(this).hide()
})
