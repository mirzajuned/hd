$("#invoice-modal").html('<%= escape_javascript(render "new.html.erb") %>')

$(".tax-rate-type").select2();

function new_tax_rate(){
  $("#new_tax_rate").validate({
    rules: {
      "tax_rate[name]": {
        required: true,
      },
      "tax_rate[rate]": {
        required: true,
      },
      "tax_rate[type]": {
        required: true,
      }
    },
    messages: {
      "tax_rate[name]": {
        required: "Name is Required."
      },
      "tax_rate[rate]": {
        required: "Rate is Required",
      },
      "tax_rate[type]": {
        required: "Type is Required"
      }
    },
    errorPlacement: function (error, element) {
      error.insertAfter(element);
    }
  });
}
new_tax_rate()