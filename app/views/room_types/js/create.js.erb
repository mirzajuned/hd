var counter = "<%= @room_type_name_counter %>"
var ward_room_type_id_field = "<%= @ward_room_type_id %>"
var room_type_id = "<%= @room_type.id.to_s %>"
jQuery(ward_room_type_id_field).val(room_type_id)
var ward_room_type_buttons = "#" + "ward_rooms_attributes_" + counter + "_room_type_buttons"
jQuery(ward_room_type_buttons).hide()
var room_type_details = "#" + "ward_rooms_attributes_" + counter + "_room_type_details"
jQuery(room_type_details).show()