$("#laboratory-set-modal").html("<%= escape_javascript(render 'form') %>")

function radiology_set_form() {
  $("#new-radiology-set-form").validate({
    rules: {
      "radiology_laboratory_set[name]": {
        required: true,
      },
      "radiology_laboratory_set[specialty_id]": {
        required: true,
      }
    },
    messages: {
      "radiology_laboratory_set[name]": {
        required: "Set Name is Required"
      },
      "radiology_laboratory_set[specialty_id]": {
        required: 'Please select any Specialty',
      }
    },
    errorPlacement: function (error, element) {
      error.insertAfter(element);
    }
  });
}
radiology_set_form()