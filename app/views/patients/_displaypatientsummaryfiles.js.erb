jQuery(".patient-prescriptions-tab").on("click", function(event) {
    event.preventDefault();
    var params = { patientid: jQuery(this).data("patientid") }
    jQuery.ajax({
        type: "GET",
        dataType: "script",
        url: "/patients/display_prescriptions",
        data: {ajax: params}
    });
});
