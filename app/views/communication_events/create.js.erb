<%= render partial: 'index.js.erb' %>
<% if flash[:error].present? %>
  notice = new PNotify({ title: 'Error', text: "<%= flash[:error] %>", type: 'error' });
<% else %>
  $("#manage_whatsapp_settings").html("<%= escape_javascript(render 'index.html.erb', locals:{module_name: params[:communication_event][:module_name]}) %>")
  $("#communication-event-modal").modal("hide")
<% end %>
notice.get().click(function(){ notice.remove() });
