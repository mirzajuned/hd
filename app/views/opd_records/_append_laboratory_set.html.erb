<% @data.each do |key, value| %>
  <% key = key.to_i + params[:row_count].to_i %>

  <tr class="addedlaboratoryinvestigationlist">
    <td style="padding-left:10px;padding-top:1px;padding-bottom:1px;padding:0px;" id="">
      <div class="rdio rdio-primary" id="">
        <input class="optional form-control investigationstage" id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigationstage_A" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][investigationstage]" type="radio" checked="checked" value="A">
        <label for="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigationstage_A" id="">Advised</label>
      </div>
      <div class="rdio rdio-primary" id="">
        <input class="optional form-control investigationstage" id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigationstage_P" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][investigationstage]" type="radio" value="P">
        <label for="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigationstage_P" id="">Performed</label>
      </div>
    </td>
    <td style="padding-left:1px;padding-top:1px;padding-bottom:1px;padding:0px;" id="">
      <div class="form-group" id="">
        <div class="input-group mb15" id="">
          <input id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>destroy" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][_destroy]" type="hidden" value="0">
          <!-- Entered Details -->
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][entered_by_id]", @current_user.id %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][order_item_advised_id]", BSON::ObjectId.new().to_s %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][entered_by]", @current_user.fullname  %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][entered_at_facility_id]", @current_facility.id %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][entered_at_facility]", @current_facility.name %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][entered_datetime]", Time.current.strftime('%d/%m/%Y %I:%M %p') %>
          <!-- Advised Details -->
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][advised_by_id]", @consultant.id, class: 'advised_by_id' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][advised_by]", @consultant.fullname, class: 'advised_by' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][advised_at_facility_id]", @current_facility.id, class: 'advised_at_facility_id' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][advised_at_facility]", @current_facility.name, class: 'advised_at_facility' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][advised_datetime]", Time.current.strftime('%d/%m/%Y %I:%M %p'), class: 'advised_datetime' %>
          <!-- Performed Details -->
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][is_performed]", false, class: 'is_performed' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][performed_by_id]", "", class: 'performed_by_id' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][performed_by]", "", class: 'performed_by' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][performed_at_facility_id]", "", class: 'performed_at_facility_id' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][performed_at_facility]", "", class: 'performed_at_facility' %>
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][performed_datetime]", "", class: 'performed_datetime' %>
          <!--  -->
          <%= hidden_field_tag "opdrecord[addedlaboratoryinvestigationlist_attributes][#{key}][investigation_type]", value['investigation_type'] %>
          <input class="string optional form-control" id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigationname" maxlength="50" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][investigationname]" size="40" type="text" value="<%= value['investigationname'] %>">
          <input id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_loinc_class" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][loinc_class]" type="hidden" value="<%= value['loinc_class'] %>">
          <input id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_loinc_code" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][loinc_code]" type="hidden" value="<%= value['loinc_code'] %>">
          <input id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigation_id" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][investigation_id]" type="hidden" value="<%= value['investigation_id'] %>">
          <input id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigation_performed" class="opdrecord_investigation_performed form-control" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][investigation_performed]" type="hidden" value="">
        </div>
      </div>
    </td>
    <td style="padding-left:1px;padding-top:1px;padding-bottom:1px;padding:0px;" id="">
      <div class="form-group" id="">
        <a id="addedlaboratoryinvestigationdeletebutton<%= key %>" class="btn btn-xs btn-primary btn-danger" href="javascript:;" name="addedlaboratoryinvestigationdeletebutton">
          <span class="glyphicon glyphicon-remove" id=""></span>
        </a>
        <input class="form-control" id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigationadviseddate" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][investigationadviseddate]" type="hidden" value="<%= Time.current.strftime('%d/%m/%Y %I:%M %p') %>">
        <input class="form-control" id="opdrecord_addedlaboratoryinvestigationlist_attributes_<%= key %>_investigationfullcode" name="opdrecord[addedlaboratoryinvestigationlist_attributes][<%= key %>][investigationfullcode]" type="hidden" value="N">
      </div>
    </td>
  </tr>
<% end %>