  jQuery(".opdrecord_traumaregion").on("click", function() {
    children.data = [];
    children1.data = [];
    children2.data = [];
    var tree_level_parent_html = "";
    var tree_level_html = "";
    var tree_root_level_html = "";
    var clicked_button_input_value = jQuery(this).attr("input-value");
