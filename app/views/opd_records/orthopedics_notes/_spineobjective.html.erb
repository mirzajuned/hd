  </div>

  <div class="row">
    <div class="col-md-2">Positive root tension signs :</div>
    <%= f.input :positiveroottensionsigns, wrapper_html: {:class=>"col-md-10"},collection: ['None','SLRT', 'Crossed SLRT', 'Lasegues', 'Femoral N. stretch', 'Spurlings'], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white positiveroottensionsigns_buttons', :input_html => {:name => "opdrecord[positiveroottensionsigns]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:positiveroottensionsigns)) || ['None'] %>
  </div>

  <div class="row">
    <div class="col-md-2">SI joint pathology :</div>
    <%= f.input :sijointpathology, wrapper_html: {:class=>"col-md-5"},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white', :input_html => {:name => "opdrecord[sijointpathology]" }, :checked => opdrecord.read_attribute(:sijointpathology) || "No" %>
