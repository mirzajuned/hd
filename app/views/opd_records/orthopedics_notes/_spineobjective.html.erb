<div class="step-pane" data-step="2">

  <div class="row">
    <div class="col-md-2">General Signs :</div>
    <%= f.input :generalsigns, wrapper_html: {:class=>"col-md-10"}, collection: ['Hyperlaxity','RA','Kochs', 'Skeletal dysplasia', 'Neurofibromatosis', 'Klippel-Feil syndrome', 'Non-musculoskeletal primary malignancy'], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white', :input_html => {:name => "opdrecord[generalsigns]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:generalsigns)) %>
  </div>

  <div class="row">
    <div class="col-md-2">Level:</div>
    <%= f.input :level, wrapper_html: {:class=>"col-md-10"},collection: ['Atlanto-occipital', 'Atlanto-axial', 'Cervical', 'Cervico-dorsal', 'Dorsal', 'Dorso-lumbar', 'Lumbar', 'Lumbo-sacral', 'Sacro-iliac', 'Coccyygeal'], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white', :input_html => {:name => "opdrecord[level]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:level)) %>
  </div>

  <div class="row">
    <div class="col-md-2">Skin condition :</div>
    <%= f.input :skincondition, wrapper_html: {:class=>"col-md-10"},collection: ['Normal', 'Tense', 'Contused', 'Abrasion', 'Wound', 'Surgical scar', 'Sinus'], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white skincondition_buttons', :input_html => {:name => "opdrecord[skincondition]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:skincondition)) || ["Normal"] %>
  </div>

  <div class="row">
    <div class="col-md-2"></div>
    <%= f.input :examinationsigns, wrapper_html: {:class=>"col-md-10"},collection: ['Warmth', 'Tenderness', 'Paraspinal spasm', 'Soft swelling', 'Knuckle', 'Gibbus', 'List', 'Scapular asymmetry', 'Pelvic asymmetry'], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white examinationsigns_buttons', :input_html => {:name => "opdrecord[examinationsigns]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:examinationsigns)) %>
  </div>

  <div class="row">
    <div class="col-md-2">Deformity :</div>
    <%= f.input :deformity, wrapper_html: {:class=>"col-md-10"},collection: ['None','Increased kyphosis','Decreased kyphosis','Increased lordosis', 'Decreased lordosis', 'Scoliosis', 'Rib hump'], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white deformity_buttons', :input_html => {:name => "opdrecord[deformity]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:deformity)) || ["None"] %>
  </div>

  <div class="row">
    <div class="col-md-2">Movements :</div>
    <%= f.input :movements, wrapper_html: {:class=>"col-md-10"},collection: ['Free','Restricted', 'Painful'], :as => :hg_radio_button,:label => false, :required => false,:class => 'btn btn-xs btn-white', :input_html => {:name => "opdrecord[movements]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:movements)) %>
  </div>

  <div class="row">
    <div class="col-md-2"></div>
