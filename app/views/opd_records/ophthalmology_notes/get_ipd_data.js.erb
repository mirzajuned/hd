var procedure_count = "<%= @procedures.count %>"
var command = "<%= @command %>"

if (procedure_count > 0) {
  if (command == "append") {
    jQuery(".post-op-record-table-body-added").append('<%= escape_javascript(render "opd_records/ophthalmology_notes/fill_ipd_data") %>')    
  }else{
    jQuery(".post-op-record-table-body-added").html('<%= escape_javascript(render "opd_records/ophthalmology_notes/fill_ipd_data") %>')  
  }
}

_renumber_id_after_remove("tbody.post-op-record-table-body-added", "div.post-op-record-table-body-removed");