<div class="step-pane <%= stepcount==1? "active" : "" %>" data-step="<%=stepcount%>">
  <div id="hg-sticky-anchor"></div>

  <div class="row mb5">
    <div class="col-md-12 col-sm-12">
      <div class="col-md-2 col-sm-2">
        Head Posture :
        <span>
          <small style="cursor: pointer;color: rgb(66, 139, 202);display: inline;border: 1px solid rgb(66, 139, 202);border-radius: 2px;" id="clear_head_posture">clear</small>
        </span>
      </div>
      
      <%= f.input :head_posture, wrapper_html: {:class=>"col-md-9 col-sm-9"}, collection: ['Chin Elevation', 'Chin Depression'], :as => :hg_radio_button,:label => false, :required => false,:class => 'btn btn-xs btn-white head_posture', :input_html => {:name => "opdrecord[head_posture]" }, :checked => opdrecord.read_attribute(:head_posture) %>
    </div>
  </div>

  <div class="row mb5">
    <div class="col-md-12 col-sm-12">
      <div class="col-md-2 col-sm-2 ">
        Face Turn :
        <span>
          <small style="cursor: pointer;color: rgb(66, 139, 202);display: inline;border: 1px solid rgb(66, 139, 202);border-radius: 2px;" id="clear_face_turn">clear</small>
        </span>
      </div>
      <%= f.input :face_turn, wrapper_html: {:class=>"col-md-9 col-sm-9"}, collection: ['Right', 'Left'], :as => :hg_radio_button,:label => false, :required => false,:class => 'btn btn-xs btn-white face_turn', :input_html => {:name => "opdrecord[face_turn]" }, :checked => opdrecord.read_attribute(:face_turn) %>
    </div>
  </div>

  <div class="row mb5">
    <div class="col-md-12 col-sm-12">
      <div class="col-md-2 col-sm-2">
        Head Tilt :
        <span>
          <small style="cursor: pointer;color: rgb(66, 139, 202);display: inline;border: 1px solid rgb(66, 139, 202);border-radius: 2px;" id="clear_head_tilt">clear</small>
        </span>
      </div>
      <%= f.input :head_tilt, wrapper_html: {:class=>"col-md-9 col-sm-9"}, collection: ['Normal', 'Abnormal'], :as => :hg_radio_button,:label => false, :required => false,:class => 'btn btn-xs btn-white head_tilt', :input_html => {:name => "opdrecord[head_tilt]" }, :checked => opdrecord.read_attribute(:head_tilt) %>
    </div>
  </div>


  <div class="row eye-row">

    <div class="col-md-6 col-sm-6 r-eye-column" style="padding-left:1px; padding-right:1px;">
      <div class="panel panel-success r-eye-panel">
        <div class="panel-heading r-eye-panel-heading" style="padding: 2px;">
          <h4 class="panel-title r-eye-panel-title text-center" style="color:#FFF;">R / OD </h4>
        </div>

        <div class="row mb5 col-md-12 col-sm-12 common_margin">          
          <div class="col-md-3 col-sm-3">
            Hirschberg Test: 
          <span>
            <small style="cursor: pointer;color: rgb(66, 139, 202);display: inline;border: 1px solid rgb(66, 139, 202);border-radius: 2px;" id="clear_r_hirschberg_test">clear</small>
          </span>
          </div>
          <div class="col-md-9 col-sm-9">
            <%= f.input :r_hirschberg_test, wrapper_html: {:class => ""}, collection: ['Normal', '7 * Eso','7 * Exo','15 * Eso','15 * Exo','30 * Eso','30 * Exo','45 * Eso','45 * Exo'], :as => :hg_radio_button, :label => false, :required => false, :class => 'btn btn-xs btn-white r_hirschberg_test', :input_html => {:name => "opdrecord[r_hirschberg_test]" }, :checked => opdrecord.read_attribute(:r_hirschberg_test) %>
          </div>
        </div>

        <div class="row col-md-12 col-sm-12 common_margin">
          <div class="col-sm-3 col-sm-3 ">Cover Test :</div>
          <span class="col-md-9 col-sm-9"><%= f.input :r_cover_test, :label => false, :required => false, :input_html => {:class => "form-control r_cover_test small_text_style", :name => "opdrecord[r_cover_test]", :size => "50", :maxlength => "90", :value => opdrecord.read_attribute(:r_cover_test) } %></span>
        </div>

        <div class="row col-md-12 col-sm-12 common_margin">
          <div class="col-md-3 col-sm-3">Alternate Cover Test </div>
          <span class="col-md-9 col-sm-9"><%= f.input :r_alternate_cover_test, :label => false, :required => false, :input_html => {:class => "form-control r_alternate_cover_test small_text_style", :name => "opdrecord[r_alternate_cover_test]", :size => "50", :maxlength => "90", :value => opdrecord.read_attribute(:r_alternate_cover_test) } %></span>
        </div>

        <div class="row mb5 col-md-12 col-sm-12 common_margin">          
          <div class="col-md-3 col-sm-3">
            Patterns: 
            <span>
              <small style="cursor: pointer;color: rgb(66, 139, 202);display: inline;border: 1px solid rgb(66, 139, 202);border-radius: 2px;" id="clear_r_patterns">clear</small>
            </span>
          </div>
          <%= f.input :r_patterns, wrapper_html: {:class => "col-md-9 col-sm-9"}, collection: [ 'A - Eso','A - Exo','V - Eso','V - Exo','Y','λ','X'], :as => :hg_radio_button, :label => false, :required => false, :class => 'btn btn-xs btn-white r_patterns', :input_html => {:name => "opdrecord[r_patterns]" }, :checked => opdrecord.read_attribute(:r_patterns) %>
        </div>

        <div class="row mb5 col-md-12 col-sm-12 common_margin">
          <div class="col-md-3 col-sm-3">
            <div>Prism bar cover test:</div>
