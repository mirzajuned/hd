            <%= f.input :r_prism_bar_cover_test, wrapper_html: {:class => ""}, collection: [ 'Esotropia','Exotropia','Hypertropia','Hypotropia','Esophoria','Exophoria','Hyperphoria','Hypophoria'], :as => :hg_radio_button, :label => false, :required => false, :class => 'btn btn-xs btn-white r_prism_bar_cover_test', :input_html => {:name => "opdrecord[r_prism_bar_cover_test]" }, :checked => opdrecord.read_attribute(:r_prism_bar_cover_test) %>
          </div>
        </div>

