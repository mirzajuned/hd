              <small style="cursor: pointer;color: rgb(66, 139, 202);display: inline;border: 1px solid rgb(66, 139, 202);border-radius: 2px;" id="clear_r_prism_bar_cover_test">clear</small>
            </span>
          </div>
          <% num_array = [] %>
          <% (5..75).to_a.select{|e| (num_array << e  if e%5 == 0) } %>
          
          <div class="col-md-2 col-sm-2">
            <%= f.select :r_prism_bar_cover_test_select, options_for_select(num_array.map{ |test_number| [test_number, test_number] }, opdrecord.read_attribute(:r_prism_bar_cover_test_select)), {}, {:class => "form-control"} %>
