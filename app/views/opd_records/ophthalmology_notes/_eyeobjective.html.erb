<div class="step-pane <%= stepcount==1? "active" : "" %>" data-step="<%=stepcount%>" style="<%= text_font_size %>">
  <div id="hg-sticky-anchor"></div>
  <div class="row mb5">
    <div class="col-md-12 col-sm-12">
      <div class="col-md-2 col-sm-2">Comments :</div>
      <div class="col-md-6 col-sm-5 generalexaminationcommoncomments-input">
        <%= f.text_area :generalexaminationcommoncomments, label: false, required: false, class: "form-control generalexaminationcommoncomments", style: 'width: 90% !important; height: 75px !important;', name: "opdrecord[generalexaminationcommoncomments]", size: "60", maxlength: "1000", value: opdrecord.read_attribute(:generalexaminationcommoncomments)  %>
      </div>
    </div>
  </div>
  <div class="row mb5">
    <div class="col-md-12 col-sm-12">
      <div class="col-md-2 col-sm-2">General Examination :</div>
      <%= f.input :generalexamination, wrapper_html: {:class=>"col-md-3 col-sm-4"}, collection: ['Normal', 'Abnormal'], :as => :hg_radio_button,:label => false, :required => false,:class => 'btn btn-xs btn-white generalexamination', :input_html => {:name => "opdrecord[generalexamination]" }, :checked => opdrecord.read_attribute(:generalexamination) || 'Normal' %>
      <div class="col-md-6 col-sm-5 generalexaminationcomments-input">
        <%= f.input :generalexaminationcomments, :label => false, :required => false, :input_html => {:class => "form-control generalexaminationcomments", :name => "opdrecord[generalexaminationcomments]", :size => "60", :maxlength => "100", :value => opdrecord.read_attribute(:generalexaminationcomments) } %>
      </div>
    </div>
    <!--<div class="col-md-2">Local Examination :</div>-->
    <!--<%#= f.input :local_examination_normal, wrapper_html: {:class=>"col-md-2 pull-right"},collection: [ ['Normal','normal'] ], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white local_examination_normal', :input_html => {:name => "opdrecord[local_examination_normal]"}, :style => "padding: 0.5px 10px;", :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:local_examination_normal)) %>-->
  </div>

  <div class="row mb5">
    <div class="col-md-12 col-sm-12">
      <div class="col-md-2 col-sm-2">One Eyed :</div>
      <%= f.input :one_eyed, wrapper_html: {:class=>"col-md-3 col-sm-3"}, collection: ['Yes', 'No'], :as => :hg_radio_button,:label => false, :required => false,:class => 'btn btn-xs btn-white one_eyed', :input_html => {:name => "patient[one_eyed]" }, :checked => @patient.read_attribute(:one_eyed) || 'No' %>
    </div>
  </div>

  <% if @templatetype == "eye" || @templatetype == "vision" %>
    <div class="row mb5">
      <div class="col-md-12 col-sm-12">
        <div class="col-md-2 col-sm-2">Squint Evaluation :</div>
        <%= f.input :squint_evaluation, wrapper_html: {:class=>"col-md-3 col-sm-3"}, collection: ['Yes', 'No'], :as => :hg_radio_button,:label => false, :required => false,:class => 'btn btn-xs btn-white squint_evaluation', :input_html => {:name => "opdrecord[squint_evaluation]" }, :checked => opdrecord.read_attribute(:squint_evaluation) || 'No' %>
      </div>
    </div>
    <div class="row mb5 squint_examination">
      <div class="col-md-12 col-sm-12">
        <div class="col-md-2 col-sm-2 mb20">Squint Examination :</div>
        <%= render partial: 'opd_records/ophthalmology_notes/squint_form.html.erb', locals: { f: f, opdrecord: @opdrecord } %>
      </div>
    </div>
  <% end %>

  <% if @templatetype == "paediatrics" %>
      <div class="row general-paediatric-info">

        <div class="col-md-12 col-sm-12">
          <div class="col-md-2 col-sm-2">Height :</div>
          <span class="col-md-1 col-sm-1"><%= f.input :paediatric_height, :label => false, :required => false, :input_html => {:class => "form-control paediatric_height small_text_style", :name => "opdrecord[paediatric_height]", :size => "10", :maxlength => "20", :value => opdrecord.read_attribute(:paediatric_height) } %></span><span class="col-md-1 col-sm-1" style="font-size: large;">cms</span>

          <div class="col-md-1 col-sm-2">Weight :</div>
          <span class="col-md-1 col-sm-1"><%= f.input :paediatric_weight, :label => false, :required => false, :input_html => {:class => "form-control paediatric_weight small_text_style", :name => "opdrecord[paediatric_weight]", :size => "10", :maxlength => "20", :value => opdrecord.read_attribute(:paediatric_weight) } %></span><span class="col-md-1 col-sm-1" style="font-size: large;">Kgs</span>

        </div>

        <div class="col-md-12 col-sm-12">
          <div class="col-md-2 col-sm-2">General Appearance :</div>
          <span class="col-md-6 col-sm-6"><%= f.input :paediatric_general_appearance, :label => false, :required => false, :input_html => {:class => "form-control paediatric_general_appearance small_text_style", :name => "opdrecord[paediatric_general_appearance]", :size => "80", :maxlength => "90", :value => opdrecord.read_attribute(:paediatric_general_appearance) } %></span>

        </div>

        <div class="col-md-12 col-sm-12">
          <div class="col-md-2 col-sm-2">Head Posture :</div>
          <span class="col-md-6 col-sm-6"><%= f.input :paediatric_head_posture, :label => false, :required => false, :input_html => {:class => "form-control paediatric_head_posture small_text_style", :name => "opdrecord[paediatric_head_posture]", :size => "80", :maxlength => "90", :value => opdrecord.read_attribute(:paediatric_head_posture) } %></span>
        </div>
        <div class="col-md-12 col-sm-12">
          <div class="col-md-2 col-sm-2">Nutritional Status :</div>
          <span class="col-md-6 col-sm-6"><%= f.input :paediatric_nutritional_status, :label => false, :required => false, :input_html => {:class => "form-control paediatric_nutritional_status small_text_style", :name => "opdrecord[paediatric_nutritional_status]", :size => "80", :maxlength => "90", :value => opdrecord.read_attribute(:paediatric_nutritional_status) } %></span>
        </div>


      </div>
  <% end %>

  <% if @templatetype == "lens" %>
      <div class="row general-lens-info">
        <div class="col-md-12 col-sm-12">
          <div class="col-md-3 col-sm-4"> Explained care & maintenance and given the emergency contactnumber :</div>
          <span class="col-md-6 col-sm-8"><%= f.text_area :lens_emergency_status, :label => false, :required => false,:class => "form-control lens_emergency_status small_text_style", :size => "80", :maxlength => "90", :value => opdrecord.read_attribute(:lens_emergency_status) %></span>
        </div>
      </div>
  <% end %>

  <div class="row eye-row">

    <div class="col-md-6 col-sm-6 col-xs-12 r-eye-column" style="padding-left:1px; padding-right:1px;">

      <div class="panel panel-success r-eye-panel">

        <div class="panel-heading r-eye-panel-heading" style="padding: 2px;">
          <h4 class="panel-title r-eye-panel-title text-center" style="color:#FFF;">R / OD <%= f.input :r_local_examination_normal, wrapper_html: {:class=>"col-md-2 col-sm-6 pull-right"},collection: [ ['Normal','normal'] ], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white r_local_examination_normal', :input_html => {:name => "opdrecord[r_local_examination_normal]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:r_local_examination_normal)) %></h4>
        </div>

        <%#= render partial: "templates/ophthalmology/opd_partials/r_eye_optometrist_examination.html.erb", locals: {f: f, opdrecord: @opdrecord} %>
        <% if @templatetype.downcase == "trauma"%>
          <div class="row r-ocular_trauma-row">
            <div class="col-md-12 col-sm-12 r-trauma-column questionsfortrauma" id="r-questionsfortrauma">
              <strong class="col-md-4 col-sm-5 badge badge-eye-template-labels">OCULAR TRAUMA </strong>
              <a id="buttontoggle-opdrecord_r_ocular_trauma" class="btn btn-xs btn-primary btn-square buttontoggle-opdrecord" href="javascript:;" name="options" ><span class="glyphicon glyphicon-plus-sign"></span></a>
              <%= f.input :r_ocular_trauma, :label => false, :required => false,:type => "hidden", :input_html => {:class => "form-control r_ocular_trauma hidden", :name => "opdrecord[r_ocular_trauma]", :size => "60",  :value => opdrecord.read_attribute(:r_ocular_trauma) || 'Normal' } %>
            </div>
            <%= render partial: "r_ocular_trauma_temp.html.erb", locals: {f: f, opdrecord: @opdrecord}%>
          </div>
        <% end %>
        <div class="hr-line-border mb5"></div>
        <div class="row r-appearance-row">
          <div class="col-md-12 col-sm-12 r-appearance-column questionsforappearance" id="r-questionsforappearance">
            <strong class="col-md-4 col-sm-5 badge badge-eye-template-labels">APPEARANCE </strong>
            <a id="buttontoggle-opdrecord_r_appearance" class="btn btn-xs btn-primary btn-square buttontoggle-opdrecord" href="javascript:;" name="options" ><span class="glyphicon glyphicon-plus-sign"></span></a>


            <%= f.input :r_appearance, :label => false, :required => false,:type => "hidden", :input_html => {:class => "form-control r_appearance hidden", :name => "opdrecord[r_appearance]", :size => "60",  :value => opdrecord.read_attribute(:r_appearance) || 'Normal' } %>
          </div>

          <div class="col-md-12 col-sm-12 r-answersforappearance">


            <div class="row col-md-12 col-sm-12">

              <span class="col-md-3 col-sm-3">Phthisis Bulbi</span>
              <span class="col-md-3 col-sm-3">Anophthalmos</span>
              <span class="col-md-3 col-sm-3">Micropththalmos</span>
              <span class="col-md-3 col-sm-3">Artificial</span>

            </div>

            <div class="row col-md-12 col-sm-12">

              <span class="col-md-3 col-sm-3"><%= f.input :r_appearance_phthisisbulbi, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appearance_phthisisbulbi', :input_html => {:name => "opdrecord[r_appearance_phthisisbulbi]" }, :checked => opdrecord.read_attribute(:r_appearance_phthisisbulbi) || "No" %></span>
              <span class="col-md-3 col-sm-3"><%= f.input :r_appearance_anophthalmos, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appearance_anophthalmos', :input_html => {:name => "opdrecord[r_appearance_anophthalmos]" }, :checked => opdrecord.read_attribute(:r_appearance_anophthalmos) || "No" %></span>
              <span class="col-md-3 col-sm-3"><%= f.input :r_appearance_micropththalmos, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appearance_micropththalmos', :input_html => {:name => "opdrecord[r_appearance_micropththalmos]" }, :checked => opdrecord.read_attribute(:r_appearance_micropththalmos) || "No" %></span>
              <span class="col-md-3 col-sm-3"><%= f.input :r_appearance_artificial, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appearance_artificial', :input_html => {:name => "opdrecord[r_appearance_artificial]" }, :checked => opdrecord.read_attribute(:r_appearance_artificial) || "No" %></span>

            </div>

            <div class="row col-md-12 col-sm-12">

              <span class="col-md-3 col-sm-3">Proptosis</span>
              <span class="col-md-3 col-sm-3">Dystopia</span>
              <span class="col-md-3 col-sm-3">Injured</span>
              <span class="col-md-3 col-sm-3">Swollen</span>

            </div>

            <div class="row col-md-12 col-sm-12">

              <span class="col-md-3 col-sm-3"><%= f.input :r_appearance_proptosis, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appearance_proptosis', :input_html => {:name => "opdrecord[r_appearance_proptosis]" }, :checked => opdrecord.read_attribute(:r_appearance_proptosis) || "No" %></span>
              <span class="col-md-3 col-sm-3"><%= f.input :r_appearance_dystopia, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appearance_dystopia', :input_html => {:name => "opdrecord[r_appearance_dystopia]" }, :checked => opdrecord.read_attribute(:r_appearance_dystopia) || "No" %></span>
              <span class="col-md-3 col-sm-3"><%= f.input :r_appearance_injured, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appearance_injured', :input_html => {:name => "opdrecord[r_appearance_injured]" }, :checked => opdrecord.read_attribute(:r_appearance_injured) || "No" %></span>
              <span class="col-md-3 col-sm-3"><%= f.input :r_appearance_swollen, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appearance_swollen', :input_html => {:name => "opdrecord[r_appearance_swollen]" }, :checked => opdrecord.read_attribute(:r_appearance_swollen) || "No" %></span>

            </div>
            <div class="row mb5">
              <div class="col-md-2 col-sm-3 text-center">Comments</div>
              <div class="col-md-9 col-sm-9">
                <%= f.input :r_appearance_comments, as: :text, :label => false, :required => false, :input_html => {:class => 'form-control opdrecord_summernote', :name => "opdrecord[r_appearance_comments]", :value => @opdrecord.read_attribute(:r_appearance_comments) } %>
              </div>
            </div>
          </div>

        </div>
        <div class="hr-line-border mb5"></div>
        <!-- rSCLERA -->
        <div class="row r-sclera-row">
          <div class="col-md-12 col-sm-12 r-sclera-column questionsforsclera" id="r-questionsforsclera">
            <strong class="col-md-4 col-sm-5 badge badge-eye-template-labels">SCLERA </strong>
            <a id="buttontoggle-opdrecord_r_scleracommon" class="btn btn-xs btn-primary btn-square buttontoggle-opdrecord" href="javascript:;" name="options" ><span class="glyphicon glyphicon-plus-sign"></span></a>
            <%= f.input :r_scleracommon, :label => false, :required => false,:type => "hidden", :input_html => {:class => "form-control r_scleracommon hidden", :name => "opdrecord[r_scleracommon]", :size => "60",  :value => opdrecord.read_attribute(:r_scleracommon) || 'Normal' } %>
          </div>
          <div class="col-md-12 col-sm-12 r-answersforsclera">
            <div class="row mb5">
              <div class="col-md-2 col-sm-3 text-center">Comments</div>
              <div class="col-md-9 col-sm-9">
                <%= f.input :r_scleracommon_comments, as: :text, label: false, required: false, input_html: { class: 'form-control opdrecord_summernote', name: "opdrecord[r_scleracommon_comments]", :value => @opdrecord.read_attribute(:r_scleracommon_comments) } %>
              </div>
            </div>
          </div>
        </div>
        <div class="hr-line-border mb5"></div>
        <!-- EOF rSCLERA -->
        <% if @templatetype.downcase == "trauma"%>
          <%= render partial: "trauma_template.html.erb", locals: {f: f, opdrecord: @opdrecord} %>
        <% else %>
          <div class="row r-appendages-row">
            <div class="col-md-12 col-sm-12 r-appendages-column questionsforappendages" id="r-questionsforappendages">
              <strong class="col-md-4 col-sm-5  badge badge-eye-template-labels">APPENDAGES </strong>
              <a id="buttontoggle-opdrecord_r_appendages" class="btn btn-xs btn-primary btn-square buttontoggle-opdrecord" href="javascript:;" name="options" ><span class="glyphicon glyphicon-plus-sign"></span></a>
              <%= f.input :r_appendages, :label => false, :required => false,:type => "hidden", :input_html => {:class => "form-control r_appendages hidden", :name => "opdrecord[r_appendages]", :size => "60",  :value => opdrecord.read_attribute(:r_appendages) || 'Normal' } %>
            </div>
            <div class="col-md-12 col-sm-12 r-answersforappendages">
              <div class="col-md-2 col-sm-2"></div>
              <%= f.input :r_appendages_tests, wrapper_html: {:class=>"col-md-10 col-sm-10"},collection: [ ['Eyelids','eyelids'], ['Eyelashes','eyelashes'], ['Lacrimal sac','lacrimalsac'], ['Syringing','syringing'] ], :as => :hg_checkbox,:label => false, :required => false,:class => 'btn btn-xs btn-white r_appendages_tests', :input_html => {:name => "opdrecord[r_appendages_tests]" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:r_appendages_tests)) %>
            </div>

            <div class="col-md-12 col-sm-12 r-appendages-eyelids r-answersforappendages-eyelids">
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3">Chalazion</span>
                <span class="col-md-3 col-sm-3">Ptosis</span>
                <span class="col-md-3 col-sm-3">Swelling</span>
                <span class="col-md-3 col-sm-3">Entropion</span>
              </div>
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelids_chalazion, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelids_chalazion', :input_html => {:name => "opdrecord[r_appendages_eyelids_chalazion]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelids_chalazion) || "No" %></span>
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelids_ptosis, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelids_ptosis', :input_html => {:name => "opdrecord[r_appendages_eyelids_ptosis]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelids_ptosis) || "No" %></span>
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelids_swelling, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelids_swelling', :input_html => {:name => "opdrecord[r_appendages_eyelids_swelling]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelids_swelling) || "No" %></span>
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelids_entropion, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelids_entropion', :input_html => {:name => "opdrecord[r_appendages_eyelids_entropion]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelids_entropion) || "No" %></span>
              </div>
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3">Ectropion</span>
                <span class="col-md-3 col-sm-3">Mass</span>
                <span class="col-md-3 col-sm-3">Meibomitis</span>
              </div>
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelids_ectropion, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelids_ectropion', :input_html => {:name => "opdrecord[r_appendages_eyelids_ectropion]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelids_ectropion) || "No" %></span>
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelids_mass, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelids_mass', :input_html => {:name => "opdrecord[r_appendages_eyelids_mass]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelids_mass) || "No" %></span>
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelids_meibomitis, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelids_meibomitis', :input_html => {:name => "opdrecord[r_appendages_eyelids_meibomitis]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelids_meibomitis) || "No" %></span>
              </div>
            </div>

            <div class="col-md-12 col-sm-12 r-appendages-eyelashes r-answersforappendages-eyelashes">
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3">Trichiasis</span>
                <span class="col-md-3 col-sm-3">Dystrichiasis</span>
              </div>
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelashes_trichiasis, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelashes_trichiasis', :input_html => {:name => "opdrecord[r_appendages_eyelashes_trichiasis]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelashes_trichiasis) || "No" %></span>
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_eyelashes_dystrichiasis, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_eyelashes_dystrichiasis', :input_html => {:name => "opdrecord[r_appendages_eyelashes_dystrichiasis]" }, :checked => opdrecord.read_attribute(:r_appendages_eyelashes_dystrichiasis) || "No" %></span>
              </div>
            </div>

            <div class="col-md-12 col-sm-12 r-appendages-lacrimalsac r-answersforappendages-lacrimalsac">
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3">Swelling</span>
                <span class="col-md-3 col-sm-3">Roplas</span>
              </div>
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_lacrimalsac_swelling, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_lacrimalsac_swelling', :input_html => {:name => "opdrecord[r_appendages_lacrimalsac_swelling]" }, :checked => opdrecord.read_attribute(:r_appendages_lacrimalsac_swelling) || "No" %></span>
                <span class="col-md-3 col-sm-3"><%= f.input :r_appendages_lacrimalsac_regurgitation, wrapper_html: {},collection: ['No', 'Yes'], :as => :hg_radio_button, :label => false,:required => false, :class => 'btn btn-xs btn-white r_appendages_lacrimalsac_regurgitation', :input_html => {:name => "opdrecord[r_appendages_lacrimalsac_regurgitation]" }, :checked => opdrecord.read_attribute(:r_appendages_lacrimalsac_regurgitation) || "No" %></span>
              </div>
            </div>

            <div class="col-md-12 col-sm-12 r-appendages-syringing r-answersforappendages-syringing">
              <div class="row col-md-12 col-sm-12">
                <span class="col-md-3 col-sm-3">Syringing</span>
              </div>
