<script>
  var canvas = $('#left_ocular_canvas')[0]
  var context = canvas.getContext('2d');
  context.lineWidth = 4;
  context.strokeStyle="#313c38";

  context.beginPath();

  context.moveTo(0, 0);
  context.lineTo(100, 100);

  context.moveTo(0, 100);
  context.lineTo(100, 0);
  context.stroke();

  var canvas = $('#right_ocular_canvas')[0]
  var context = canvas.getContext('2d');
  context.lineWidth = 4;
  context.strokeStyle="#313c38";

  context.beginPath();

  context.moveTo(0, 0);
  context.lineTo(100, 100);

  context.moveTo(0, 100);
  context.lineTo(100, 0);

  context.stroke();
</script>