var speciality = $('#opdrecord_specalityname').val();
var form_type = $('#opdrecord_templatetype').val();
var step_name = '';
if(form_type == 'express') {
  step_name = 'clincalevaluation_step_opthalinvestigations';
} else if(form_type == 'freeform') {
  step_name = '_plan';
} else if(form_type == 'trauma' && speciality == 'orthopedics') {
  step_name = 'trauma_assesment_step';
} else {
  step_name = 'assesment_step';
}
