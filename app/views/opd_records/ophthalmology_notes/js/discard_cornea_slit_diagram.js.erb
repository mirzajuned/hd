jQuery('.' + '<%= @eyeside %>' + '-corneaslit-diagram').html('<%= escape_javascript( render partial: "opd_records/ophthalmology_notes/opd_partials/new_corneaslit_diagram_modal", locals: { eyeside: @eyeside } ) %>')
jQuery(".addcorneaslitdiagram").on("click", function(event) {
    event.preventDefault();
    var eyeside = jQuery(this).attr("data-eye_side");
    var params = { eyeside: eyeside, diagram_type: "corneaslit", patient_id: "<%= @patient_id %>" };
    jQuery.ajax({
        type: "GET",
        url: "/opd_records/add_cornea_slit_diagram",
        data: {ajax: params, remote: true}
    });
    return false;
});

jQuery("#ajax-modal").modal("hide");