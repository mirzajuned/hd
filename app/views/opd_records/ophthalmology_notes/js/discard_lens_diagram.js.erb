jQuery('.' + '<%= @eyeside %>' + '-lens-diagram').html('<%= escape_javascript( render partial: "opd_records/ophthalmology_notes/opd_partials/new_lens_diagram_modal", locals: { eyeside: @eyeside } ) %>')

jQuery(".add_lens_diagram").on("click", function(event) {
  event.preventDefault();
  var eyeside = jQuery(this).attr("data-eye_side");
  var params = { eyeside: eyeside, diagram_type: "lens", patient_id: "<%= @patient_id %>" };

  jQuery.ajax({
    type: "GET",
    url: "/opd_records/add_lens_diagram",
    data: { ajax: params, remote: true }
  });
  return false;
});

jQuery("#ajax-modal").modal("hide");