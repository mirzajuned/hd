<div class="modal-dialog modal-hg-fileupload">
  <div class="modal-content">
    <div class="modal-header">
       <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title"><%= @eyeside.upcase %> - <%= @diagram_type.upcase %> DIAGRAM</h4>
    </div>
    <div class="modal-body">
      <center>
        <div>
          <%= image_tag "#{@image}", height: '400', width: '400' %>
