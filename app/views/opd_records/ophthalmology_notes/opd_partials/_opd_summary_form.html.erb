        <% elsif @templatetype == "ar_nct" %>
          <% if @current_facility.country_id != "VN_254" %>
            <%= render partial: "opd_records/ophthalmology_notes/opd_partials/summary/opd_summary_form_examination_refraction", locals: { specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
