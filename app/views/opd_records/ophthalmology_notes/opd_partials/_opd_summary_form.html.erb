            <%= render partial: "opd_records/ophthalmology_notes/opd_partials/summary/opd_summary_refraction", locals: {patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
