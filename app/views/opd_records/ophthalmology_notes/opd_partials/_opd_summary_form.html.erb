            <%= render partial: "opd_records/ophthalmology_notes/opd_partials/summary/opd_summary_form_examination_refraction", locals: {patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false} %>
            <%= render partial: "opd_records/ophthalmology_notes/opd_partials/summary/opd_summary_form_squint_examination", locals: {patient: @patient, appointment: @appointment, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false} %>
            <%= render partial: "opd_records/ophthalmology_notes/opd_partials/summary/opd_summary_form_examination", locals: {patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false} %>
          <% else %>
            <%= render partial: "opd_records/ophthalmology_notes/opd_partials/summary/opd_summary_refraction", locals: {patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
            <% if @opdrecord.dilate_patient %>
              <%= render partial: 'opd_records/ophthalmology_notes/opd_partials/summary/opd_summary_dilated_refraction.html.erb', locals: { patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
