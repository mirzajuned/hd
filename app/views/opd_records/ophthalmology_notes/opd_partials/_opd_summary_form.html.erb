<% if params[:showclone] == "true" %>
  <%= render "opd_records/#{@speciality_folder_name}_notes/opd_partials/clone_selective_opd_summary" %>
<% else %>
  <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/opd_summary_form_actions", locals: { patient: @patient, appointment: @appointment, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
<% end %>

<% rules_file ||= nil %>
<% if rules_file.present? %>
  <%= render partial: "templates/common/validation_status_summary", locals: { opdrecord: @opdrecord, templatetype: @templatetype, rules_file: rules_file, mail_job: false } %>
<% end %>

<div class="summary-template" style="box-shadow: 0 0 6px #ccc;max-width: 110%;padding: 0 0.400000in 0 0.550000in;padding-top: 0px;padding-right: 0.4in;padding-bottom: 0px;padding-left: 0.55in;font-family: '<%= UsersSetting.find_by(user_id: current_user.id).font_style %>';">
  <div class="row">
    <div class="col-md-12 col-sm-12">
      <div class="opdsummary">
        <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/opd_summary_form_patientinfo", locals: { patient: @patient, appointment: @appointment, opdrecord: @opdrecord, mail_job: false } %>
        <div class="pcs-entity-title"><span>Opd Summary</span></div>
        <% nursing_template = OpdRecord.find_by(appointmentid: @appointment.id.to_s, templatetype: 'nursing') %>
        <% refraction_template = OpdRecord.find_by(appointmentid: @appointment.id.to_s, templatetype: 'refraction') %>
        <% if @templatetype == "express" %>
          <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/summary/opd_summary_form_expresshistory", locals: { patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
          <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/summary/opd_summary_form_assessment", locals: { patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
          <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/summary/opd_summary_form_plan", locals: { patient: @patient, appointment: @appointment, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
          <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/summary/opd_summary_form_advice", locals: { patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, advice: @opdrecord.advice, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
        <% elsif @templatetype == "freeform" %>
          <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/summary/opd_summary_form_freeform", locals: { patient: @patient, appointment: @appointment, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
          <div class="mb5">
            <% if @opdrecord.treatmentmedication.length > 0 || @opdrecord.medicationcomments.length > 0 %>
              <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/summary/opd_summary_form_medication.html.erb", locals: { patient: @patient, appointment: @appointment, specalityfoldername: @speciality_folder_name, opdrecord: @opdrecord, templatetype: @templatetype, opdspeciality: @opdspeciality, mail_job: false, medication_groups: @medication_groups, translated_language: @translated_language, advice_language: @advice_language } %>
            <% end %>
          </div>
          <%= render partial: "opd_records/#{@speciality_folder_name}_notes/opd_partials/summary/opd_summary_form_advice", locals: { patient: @patient, specalityfoldername: @speciality_folder_name, specalityid: @specalityid, opdrecord: @opdrecord, advice: @opdrecord.advice, templatetype: @templatetype, templateid: @templateid, opdspeciality: @opdspeciality, mail_job: false } %>
