          <div class="col-md-10 col-sm-10" style="padding: 0">
            <span class="col-md-1 col-sm-1 text-right" style="margin-top: 1%">S</span>
            <%= f.input :r_visualacuity_ua_s, wrapper_html: {:class => "col-md-2 col-sm-2"},collection: ['+','-'], :include_blank => 'Sel', :label => false, :required => false, :class => 'btn btn-xs btn-white r_va r_visualacuity_ua_s form-control', :input_html => {:name => "opdrecord[r_visualacuity_ua_s]", :class => "form-control r_va r_visualacuity_ua_s", :style => "padding: 4px 0px" }, :checked => opdrecord.checkboxes_checked(opdrecord.read_attribute(:r_visualacuity_ua_s)) %>
