      $('#add_pgp').css("display","none");
    }
  }
  hide_add_pgp1()

  function hide_add_pgp2(){
    if($('.l-div-pgp3').css('display') == 'block'){
      $('#add_pgp2').css("display","none");
    }
  }
  hide_add_pgp2()

  $('#add_pgp').on('click', function(e) {
    e.preventDefault();
    $('.l-div-pgp2').show();
    $('#add_pgp').css("display","none");
  });

  $('#add_pgp2').on('click', function(e) {
    e.preventDefault();
    $('.l-div-pgp3').show();
    $('#add_pgp2').css("display","none");
  });

  $('#remove_pgp3').on('click', function(e) {
    e.preventDefault();
    $('#l_tablepgp3').find('td').text('--')
    $('#l-lens3').text('')
    $('#l_tablepgp3').find('tr:eq(1) > td:eq(0)').text('Distant')
    $('#l_tablepgp3').find('tr:eq(2) > td:eq(0)').text('Add')
    $('#l_tablepgp3').find('tr:eq(3) > td:eq(0)').text('Near')
    $('.l-answersforpgp3').find('input').val('')
    $('.l-answersforpgp3').find('#opdrecord_l_pgp_typeoflens3').val('')
    $('#opdrecord_l_pgp_comments3').val('')
    $('.l_pgp_comments3').text('')
    $('.l-div-pgp3').hide();
    $('#add_pgp2').css("display","block");
  });

