  }
  $(".l_datepicker_revisit").datepicker(datepicker_options);
  $('#opdrecord_l_intraocularpressure_time').timepicker({minuteStep: 5});

  <% if opdrecord.read_attribute(:l_revisit_date).present? %>
    $('#l_clear_revisit_date').fadeIn()
