        <%= f.input :l_visualacuity_unaided, wrapper_html: { class: 'col-md-9 col-sm-9', style: 'padding: 0px' }, collection: opdrecord.visual_acuity_values(country_id), as: :hg_radio_button, label: false, required: false, class: 'btn btn-xs btn-white l_va l_visualacuity_unaided', input_html: { name: 'opdrecord[l_visualacuity_unaided]' }, checked: opdrecord.read_attribute(:l_visualacuity_unaided) %>
