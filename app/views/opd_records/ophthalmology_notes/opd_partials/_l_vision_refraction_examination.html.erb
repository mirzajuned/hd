            <%= f.check_box :no_left_va_advised, {class: "left_va_advised", :value => opdrecord.read_attribute(:no_left_va_advised)}, "true", "false" %>
            <label for="opdrecord_no_left_va_advised">VA not examined</label>
          </div>
        </div>
      <% end %>
