$("#contact-modal").html('<%= escape_javascript(render "index.html.erb") %>')

// Validation
function contact_group_new(){
  $(".error-message-uniqueness").html("")
  $("#new-contact-group-form").validate({
    rules: {      
      "contact_group[name]": {
        required: true,
      }
    },
    messages: {
      "contact_group[name]": {
        required: "Name Cant be Blank"
      }
    },
    errorPlacement: function (error, element) {
      error.insertAfter(element);
    }
  });
}

contact_group_new()