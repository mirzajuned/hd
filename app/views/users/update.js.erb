
if(jQuery('#table_org_manage').length) {
    jQuery('#table_org_manage').dataTable().fnDraw()
    jQuery("#org-modal").html("<%= escape_javascript(render 'preview') %>")
    jQuery("#org-modal").modal("show")
} else if(jQuery('#facility_manage').length) {
    jQuery('#facility_manage').dataTable().fnDraw()
    jQuery("#org-modal").html("<%= escape_javascript(render 'preview') %>")
    jQuery('#user_manage').dataTable().fnDraw()
    jQuery("#org-modal").modal("show")
}
