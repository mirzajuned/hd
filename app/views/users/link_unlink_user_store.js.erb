$("#user-modal").html("<%= escape_javascript(render 'link_unlink_user_store.html.erb') %>")

$("#user_ids").select2({
  placeholder: 'Search by Name/EmployeeID/Designation'
})

function link_unlink_user() {

  $("#link_unlink_user_store_save").validate({
    rules: {
      "user_ids[]": {
        required: true,
      },
    },
    messages: {
      "user_ids[]": {
        required: "Select Atleast One User"
      }
    },
  });
}
link_unlink_user()