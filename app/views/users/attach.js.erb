jQuery("#org-modal").html("<%= escape_javascript(render 'attach') %>")
jQuery("#org-modal").modal("show")

function User_id_validation() {
$('#user_attach_save').validate({
    rules: {
      "user_id": {
        required: true,
      }
    },
    messages: {
      "user_id": {
        required: "Please select a name."
      }
    },
    errorPlacement: function (error, element) {
      error.insertAfter(element);
    }
  });
}
User_id_validation()
