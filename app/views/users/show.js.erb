jQuery("#user-modal").html("<%= escape_javascript(render 'show.html.erb') %>")

$('#manage_users').dataTable().fnDraw()

$(".show_more_facilities_link").on('click', function(e){
  e.preventDefault();
  $(this).hide();
  facility_name = $(".fac_name")
  for (var i = facility_name.length - 1; i >= 0; i--) {
    $(facility_name[i]).show();
  }
})