  $(".patient-note-message").val("")
  $("#patient-notes").html("<%= escape_javascript(render partial: 'outpatients/appointment/appointment_details/patient_notes.html.erb') %>");
} else {
  jQuery(".patient_note_appointment").html("<%= escape_javascript(render partial: 'opd_appointments/show_patient_note_box') %>")
  jQuery('.minimize').click(function(){
    var t = jQuery(this);
    var p = t.closest('.panel');
    if(!jQuery(this).hasClass('maximize')) {
        p.find('.panel-body, .panel-footer').slideUp(200);
        t.addClass('maximize');
        t.html('&plus;');
    } else {
        p.find('.panel-body, .panel-footer').slideDown(200);
        t.removeClass('maximize');
        t.html('&minus;');
    }
    return false;
  });
}
