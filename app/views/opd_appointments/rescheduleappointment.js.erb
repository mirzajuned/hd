jQuery("#appointment-modal").html("<%= escape_javascript(render 'rescheduleappointment') %>")

jQuery("#appointment-modal").modal("show")
jQuery(".each-appoointment-slot").on("click", function() {
    jQuery('#datepicker-inline').datepicker("setDate", moment(jQuery(this).data("appointmentdate"), "YYYY-MM-DD-").format('DD/MM/YYYY'))
    jQuery('#timepicker').timepicker('setTime', jQuery(this).data("appointmenttime"))
})