$("#appointment-modal").html("<%= escape_javascript(render 'filteropdappointmentlist.html.erb') %>")
$("#appointment-modal").modal("show")

$('.selected_doctor').on("change", function(){
  let selected_doctor = $('.selected_doctor').val()
  let _href = $(".generate_report").attr('href')
  $(".generate_report").attr("href", _href + '&doctor=' + selected_doctor);
});