<div class="contentpanel">
  <%= render "practice_setting" %>
</div>

<script>
  $(document).ready(function () {
    jQuery("#laboratorysets_ids").on('click', function() {
      jQuery.ajax({
        type: "GET",
        dataType: "script",
        data: { remote: "true"},
        url: '/medication_kits/facilitylaboratorysets'
      });
    })

    jQuery("#ophthal_sets_id").on('click', function() {
      jQuery.ajax({
        type: "GET",
        dataType: "script",
        data: { remote: "true"},
        url: '/ophthal_laboratory_sets'
      });
    })

    jQuery("#radiology_sets_id").on('click', function() {
      jQuery.ajax({
        type: "GET",
        dataType: "script",
        data: { remote: "true"},
        url: '/radiology_laboratory_sets'
      });
    })
    
    $(".sub_list_li :first").trigger('click')
  })
</script>