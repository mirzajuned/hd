$('#manage_feedback_settings_id').trigger('click')
jQuery("#feedback-form-modal").modal('hide')

function view_details(){
  var set_type = "<%= @set_type %>";
  if(set_type == 'opd'){
    $(".opd-feedback-details-show").trigger('click')
  }else if(set_type == 'discharge'){
    $(".discharge-feedback-details-show").trigger('click')
  }
}
setTimeout(view_details, 300);