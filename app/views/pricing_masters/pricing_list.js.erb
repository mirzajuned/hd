$("#pricing_master_table_content").html("<%= j render partial: 'pricing_master_list.html.erb' %>")

$("#payee-pricing-link").html(`
  <%= link_to edit_payer_pricing_pricing_masters_path(facility_id: params[:facility_id]), data: { remote: true, 'toggle': 'modal', 'target': '#service-master-modal' }, class: 'btn btn-primary navbar-btn' do %><i class="fa fa-plus"></i>&nbsp;&nbsp;Add Payee Pricing<% end %>
`)

// BACK Button
$(".back-btn").show()
$(".back-btn").on('click', function(e){
  e.preventDefault()
  let specialty_id = $("#manage_pricing_masters").find('.specialty_dropdown').val()
  $.ajax({
    type: "GET",
    dataType: "script",
    url: `/pricing_masters?specialty_id=${specialty_id}`
  });
  $(this).hide()
})

$('.search-pricing-master').show()
$('.search-facility').hide()