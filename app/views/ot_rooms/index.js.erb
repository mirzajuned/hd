$("#ot_setup_form_settings").html("<%= escape_javascript(render partial: 'index.html.erb') %>");

$("#ot_rooms_facility_dropdown").on('change', function(){
  var facility_dropdown_id = $(this).val()
   $.ajax({
    type: 'GET',
    dataType: 'script',
    data: { facility_dropdown_id: facility_dropdown_id },
    url: '/ot_rooms/facility_dropdown',
  })
});

$("#ot_rooms_facility_dropdown").select2();

