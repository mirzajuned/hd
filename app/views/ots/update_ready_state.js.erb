<% if @admission_list_view.user_id == current_user.id && params[:value] == 'true' %>
  $("#workflow-modal").html("<%= escape_javascript(render partial: 'update_ready_state.html.erb') %>").modal('show')
<% end %>

if($(".ot-panel").length > 0){
  $("#btn-ot-management-refresh").trigger('click');
} else if($(".ot-calendar-panel").length > 0){
  $("#ot-calendar").fullCalendar("refetchEvents")
}
