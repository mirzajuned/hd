<div class="col-sm-12 main-view-statistics" style="height: 40px; padding-left: 0;background-color:#eee;">
  <div id="toolbar_area">
    <div id="toolbar">
      <nav class="navbar navbar-secondary navbar-inverse navbar-fixed-top" style="margin-top: 50px; left: auto; right: auto; width: 100%;z-index:100;">
        <div class="container-fluid">
          <div class="navbar-header col-sm-2">
            <span class="navbar-brand">
              <a href="javascript:;" data-filter='All' class="statistics_parent_link_main all" style="color:#fff;text-decoration: none;">Finance Reports</a>&emsp;
              <a href="javascript:;" class="statistics_parent_link_refresh" style="color:#fff;text-decoration: none;font-size:12px;"><i class="fa fa-refresh"></i></a>
            </span>
          </div>
          <div class="col-sm-3">
            <ul class="nav navbar-nav navbar-left" style="margin-left:10px;">
              <li class="dropdown">
                <a href="#" class="dropdown-toggle hover_focus" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" style="color:#fff;"><span id="current_time_period" data-time-period="today">Today</span>&nbsp;&nbsp;<span class="fa fa-caret-down"></span></a>
                <ul class="dropdown-menu">
                  <li><a class="toolbar_time" href="#" data-time-period="today">Today</a></li>
                  <li><a class="toolbar_time" href="#" data-time-period="week">Week</a></li>
                  <li><a class="toolbar_time" href="#" data-time-period="month">Month</a></li>
                  <li><a class="toolbar_time" href="#" data-time-period="3months">3 Months</a></li>
                  <li><a class="toolbar_time" href="#" data-time-period="6months">6 Months</a></li>
                  <li><a class="toolbar_time" href="#" data-time-period="1year">1 Year</a></li>
                  <li><a class="toolbar_time" href="#" data-time-period="beginning">From Beginning</a></li>
                </ul>
              </li>
            </ul>
            <% if @facility_currencies.count > 1 %>
              <ul class="nav navbar-nav navbar-left" style="margin-left:10px;">
                <li class="dropdown">
                  <a href="#" class="dropdown-toggle hover_focus" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" style="color:#fff;"><span id="current_currency_id" data-currency_id="<%= current_facility.currency_id %>"><%= @currency.name %> - <%= current_facility.currency_symbol %></span>&nbsp;&nbsp;<span class="fa fa-caret-down"></span></a>
                  <ul class="dropdown-menu" id="main-currency-list" style="overflow-y: scroll;max-height: 520px;">
                    <!-- <li><input type="text" name="search_currency" id='search-currency' placeholder="Search Currency" class="form-control p5_10"></li> -->
                    <!-- <hr style="margin:5px;" /> -->
                    <% @facility_currencies.each do |currency| %>
                      <li class="main-currency-list"><a class="toolbar_currency" href="#" data-currency_id="<%= currency.id %>"><span class="currency_name"><%= currency.name %> - <%= currency.symbol %></span></a></li>
                    <% end %>
                  </ul>
                </li>
              </ul>
            <% else %>
              <ul class="nav navbar-nav navbar-left" style="margin-left:10px;">
                <li class="dropdown">
                  <a href="#" class="dropdown-toggle hover_focus" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" style="color:#fff;"><span id="current_currency_id" data-currency_id="<%= current_facility.currency_id %>"><%= @currency.name %> - <%= current_facility.currency_symbol %></span></a>
                </li>
              </ul>
            <% end %>
          </div>
          <!-- Collect the nav links, forms, and other content for toggling -->
          <div class="col-sm-5">
            <ul class="nav navbar-nav navbar-right">
              <li class="">
                <div class="col-sm-4">
                  <input type="text" id="report-to-datepicker" value="" class="form-control report-to-datepicker" style="margin-top: 3%;cursor:pointer;background-color:#fff;" placeholder="Start Date" readonly="readonly">
                </div>
                <div class="col-sm-1 text-center">
                  <i class="fa fa-arrows-alt-h" style="margin-top:15px;color:#fff;font-size: 20px;"></i>
                </div>
                <div class="col-sm-4">
                  <input type="text" id="report-from-datepicker" value="" class="form-control report-from-datepicker" style="margin-top: 3%;cursor:pointer;background-color:#fff;" placeholder="End Date" readonly="readonly">
                </div>
                <div class="col-sm-2">
                  <div class="btn btn-primary btn-sm" id="btnReportRange" style="margin-top:7px;"><i class="fa fa-search"></i> &nbsp;Search</div>
                </div>
              </li>
            </ul>
          </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
      </nav>
    </div>
  </div>
</div>
