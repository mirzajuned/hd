<script type="text/javascript">
    // To Prevent Dropdown from Closing On Input Focus
    $(document).on('click', '#search-currency', function (e) {
      e.stopPropagation();
    });

    // To empty Dropdown Search once a currency is selected
    $(document).on('click', '.toolbar_currency', function (e) {
      $("#search-currency").val("").trigger('keyup')
    });

    // JS SEARCH
    $("#search-currency").on('keyup', function(){
      currency_list = $(".main-currency-list");
      search = $(this).val().toUpperCase().trim();
      for (var i = currency_list.length - 1; i >= 0; i--) {
        currency_name = $(currency_list[i]).find(".currency_name")
        if ($(currency_name).html().toUpperCase().indexOf(search) > -1) {
          $(currency_list[i]).css("display", "");
        } else {
          $(currency_list[i]).css("display", "None");
        }
      }
    })
  </script>