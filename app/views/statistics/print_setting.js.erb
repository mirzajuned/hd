$("#invoice-modal").html("<%= escape_javascript(render 'print_setting.html.erb') %>")

$(".btn-print-setting").on('click', function(){
  var checked_fields = []
  var start_date = $("#start_date").val()
  var end_date = $("#start_date").val()

  var check = $(".check_checkbox")
  for (var i = check.length - 1; i >= 0; i--) {
    if($(check[i]).is(':checked')){
      checked_fields.push($(check[i]).attr('id'))
    }
  }

  window.open("/statistics/print.pdf?flag=<%= params[:flag] %>&start_date=<%= Date.parse(params[:start_date]).strftime('%d/%m/%Y') %>&end_date=<%= Date.parse(params[:end_date]).strftime('%d/%m/%Y') %>&checked_fields=" + checked_fields, '_blank');
})