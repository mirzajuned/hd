<% if @start_date == @end_date %>
  <% if @filter == "All" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/all_data.html.erb") %>')
  <% end %>
  <% if @filter == "OPD" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/opd_data_daily.html.erb") %>')
  <% end %>

  <% if @filter == "IPD" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/ipd_data_daily.html.erb") %>')
  <% end %>

  <% if @filter == "Credit" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/credit_data_daily.html.erb") %>')
  <% end %>

  <% if @filter == "Inventory" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/inventory_data_daily.html.erb") %>')
  <% end %>

  <% if @filter == "Tax" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/tax_data_daily.html.erb") %>')
  <% end %>

  <% if @filter == "Other" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/other_data_daily.html.erb") %>')
  <% end %>
<% else %>
  <% if @filter == "All" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/all_data.html.erb") %>')
  <% end %>
  <% if @filter == "OPD" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/opd_data.html.erb") %>')
  <% end %>

  <% if @filter == "IPD" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/ipd_data.html.erb") %>')
  <% end %>

  <% if @filter == "Credit" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/credit_data_daily.html.erb") %>')
  <% end %>

  <% if @filter == "Inventory" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/inventory_data.html.erb") %>')
  <% end %>

  <% if @filter == "Tax" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/tax_data_daily.html.erb") %>')
  <% end %>

  <% if @filter == "Other" %>
    $('#statistics_area').html('<%= escape_javascript(render "statistics/data_files/other_data.html.erb") %>')
  <% end %>
<% end %>

